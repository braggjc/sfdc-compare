    <validationRules>
        <fullName>BAblocker</fullName>
        <active>true</active>
        <description>https://helpdeskce.in.gtsce.com/browse/CECOL-401</description>
        <errorConditionFormula>IF(AND(OR(ISPICKVAL(OrderType__c, &quot;Service Management&quot;), 
ISPICKVAL(OrderType__c, &quot;Renewal&quot;)), OR(NOT(ISBLANK( Billing_Account__c)), NOT(ISBLANK( Billing_Account_Name__c)))) , true , false)</errorConditionFormula>
        <errorMessage>For Order Types RENEWAL and SERVICE MANAGEMENT it is not possible to select Billing Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BankNameWithAccountNumber</fullName>
        <active>true</active>
        <description>Check, if Bank name has been selected then the Bank account number also has to be filled in</description>
        <errorConditionFormula>AND(OR (AND(NOT (ISBLANK(Bank_Account_Number__c)), ISBLANK(Bank_Name__c)),AND ((ISBLANK(Bank_Account_Number__c)), NOT (ISBLANK(Bank_Name__c)))),  $RecordType.Name = &apos;Domestic Order&apos;)</errorConditionFormula>
        <errorDisplayField>Bank_Name__c</errorDisplayField>
        <errorMessage>If you select a Bank name you have to enter Bank Account Number. If you enter Bank Account Number you also have to choose Bank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillingAccountVSAccount</fullName>
        <active>true</active>
        <description>Verifies if selected BA belongs to correct Customer</description>
        <errorConditionFormula>AND(Billing_Account__c != null,  Billing_Account__r.Account__c !=  Account__c )</errorConditionFormula>
        <errorDisplayField>Billing_Account__c</errorDisplayField>
        <errorMessage>Selected Billing Account belongs to different Customer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancellation_date_removal</fullName>
        <active>true</active>
        <description>Cancelation Date can be changed but not removed</description>
        <errorConditionFormula>ISNULL(PRIORVALUE(Cancellation_Date__c)) = false &amp;&amp; ISNULL(Cancellation_Date__c) = true &amp;&amp; (ISPICKVAL( Status__c , &apos;In Cancellation&apos;) || ISPICKVAL( Status__c , &apos;Cancelled&apos;))</errorConditionFormula>
        <errorDisplayField>Cancellation_Date__c</errorDisplayField>
        <errorMessage>Cancelation Date can be changed but not removed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Change_Billing_Date_required_on_Close</fullName>
        <active>true</active>
        <description>Checks if Billing Date has been entered before closing</description>
        <errorConditionFormula>AND(ISPICKVAL( Status__c , &apos;Completed&apos;), ISNULL( Change_Billing_Customer__c ), $RecordType.Name = &apos;Service Order&apos;)</errorConditionFormula>
        <errorDisplayField>Change_Billing_Customer__c</errorDisplayField>
        <errorMessage>Change Billing Customer Date is required when closing Order</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckBankAccountForPaymentMethod</fullName>
        <active>true</active>
        <description>If as a Payement method Direct debit has been selected then Bank name, Bank account number and limit can not be empty</description>
        <errorConditionFormula>AND(PaymentMethod__r.Name = &apos;Direct Debit&apos; ,OR( ISNULL(Bank_Name__c) , ISNULL(Bank_Account_Number__c), ISNULL( Limit__c )),  $RecordType.Name = &apos;Domestic Order&apos;)</errorConditionFormula>
        <errorDisplayField>PaymentMethod__c</errorDisplayField>
        <errorMessage>If as a Payement method Direct debit has been selected then Bank name, Bank account number and limit can not be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_Address_required_for_Delivery_Type</fullName>
        <active>true</active>
        <description>Email address must be given if Invoice Delivery EMAIL selected</description>
        <errorConditionFormula>AND(OR( InvoiceDelivery__r.Value__c = &apos;EMAIL&apos;, InvoiceDelivery__r.Value__c = &apos;BOTH&apos;), ISBLANK( Invoice_Email__c ),  $RecordType.Name = &apos;Domestic Order&apos;)</errorConditionFormula>
        <errorDisplayField>Invoice_Email__c</errorDisplayField>
        <errorMessage>Please, provide email address for invoice delivery</errorMessage>
    </validationRules>
