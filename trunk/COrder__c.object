        <linkType>javascript</linkType>
        <masterLabel>Create Quotation</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 

var result = sforce.connection.query(&quot;select Id from OpportunityLineItem where OpportunityId = &apos;{!Opportunity.Id}&apos; and isDeleted = false&quot;); 

var products = result.getArray(&apos;records&apos;); 
var type = &quot;{!Opportunity.Type_Details__c}&quot;; 

if(products.length == 0){ 
alert(&apos;Please add at least one product before creating a Quotation!&apos;); 
} else if(products.length == 1 || type != &quot;New Service&quot;){ 
var res = confirm(&quot;This will create a new Quotation. Do you want to continue?&quot;); 
if(res){ 
var oppid = &quot;{!Opportunity.Id}&quot;; 
var quotation= &quot;true&quot;; 

sforce.apex.execute(&quot;OrderAutoCreate&quot;,&quot;create&quot;, {OpportunityId:oppid, isQuotation: quotation}); 

//reload the current page 
window.location.href = window.location.href; 
} 
} else { 
window.location.href = &quot;https://&quot; + window.location.host + &quot;/apex/CreateOrderWizard?id={!Opportunity.Id}&quot;; 
}</url>
