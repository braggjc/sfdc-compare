        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/apex.js&quot;)}

if (&quot;{!Account.OwnerFullName}&quot; == &quot;{!$User.FirstName}&quot; + &quot; &quot; + &quot;{!$User.LastName}&quot;) {
  var answer = confirm(&quot;Do you want to proceed with sharing related Requests and Request Quotations?&quot;);
  if (answer) {
    var result = parseInt(sforce.apex.execute(&quot;RelatedToAccount&quot;,&quot;ShareAll&quot;, {sAccountId:&quot;{!Account.Id}&quot;}));
    switch(result) {
      case 0: alert(&quot;Sharing completed successfully.&quot;); break;
      case 1: alert(&quot;All related records are already shared!&quot;); break;
      case 2: alert(&quot;Sharing unsuccessful!&quot;); break;
      default: alert(&quot;Unexpected error!&quot;); break;
    }
  }
}
else
  alert(&quot;Sharing can be executed only by the Owner of this Account!&quot;);</url>
