    <webLinks>
        <fullName>Send_to_CBCT</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send to CBCT</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
//{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)}

var result = sforce.connection.query(&quot;SELECT Id, Send_to_CBCT__c FROM Calculation__c WHERE Calculation__c.Id = &apos;{!Calculation__c.Id}&apos;&quot;);
record = result.getArray(&quot;records&quot;)[0];

if (record.Send_to_CBCT__c == &quot;false&quot;){
   var calc = new sforce.SObject(&quot;Calculation__c&quot;);
   calc.Id = &apos;{!Calculation__c.Id}&apos;;
   calc.Send_to_CBCT__c = true;
   var result = sforce.connection.update([calc]);

   if (result[0].getBoolean(&quot;success&quot;)) {
    alert(&quot;Calculation with id &quot; + result[0].id + &quot; was sent to CBCT&quot;);
