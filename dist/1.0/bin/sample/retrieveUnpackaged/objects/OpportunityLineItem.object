    <validationRules>
        <fullName>Block_Change_of_Monthly_Fee</fullName>
        <active>true</active>
        <description>Block_Change_of_Monthly_Fee</description>
        <errorConditionFormula>IF ( ISCHANGED( Total_Monthly_Fee__c ) &amp;&amp; Total_Monthly_Fee__c &lt;&gt; 0 &amp;&amp; Total_Monthly_Fee__c &lt;&gt; Monthly_Revenue_Delta__c &amp;&amp;  Opportunity.RecordTypeId  = &quot;01220000000LWd0&quot;,True, False )</errorConditionFormula>
        <errorMessage>Total Monthly Fee for Domestic Opportunities cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Block_Change_of_Quantity</fullName>
        <active>true</active>
        <description>Block_Change_of_Quantity</description>
        <errorConditionFormula>IF ( ISCHANGED( Quantity ) &amp;&amp; Quantity&lt;&gt;1 &amp;&amp; (Opportunity.RecordTypeId = &quot;01220000000LWd0&quot;|| Opportunity.RecordTypeId =&quot;01220000000LWrp&quot;) , True, False)</errorConditionFormula>
        <errorMessage>Quantity cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LineType_NewService_change_notallowed</fullName>
        <active>true</active>
        <description>Change of Line type from New Service to  any other is not allowed.</description>
        <errorConditionFormula>AND( OR  (CONTAINS( Opportunity.RecordTypeId,&quot;01220000000LWd0&quot;), CONTAINS( Opportunity.RecordTypeId,&quot;01220000000LWrp&quot;)), ISCHANGED( Line_Type__c), PRIORVALUE(Line_Type__c)&lt;&gt;TEXT(Line_Type__c) , TEXT(PriorValue(Line_Type__c))= &quot;New Service&quot; )</errorConditionFormula>
        <errorDisplayField>Line_Type__c</errorDisplayField>
        <errorMessage>Change of Line Type from New Service to any other is not allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LineType_not_NewService_cantb_NewService</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR (CONTAINS( Opportunity.RecordTypeId,&quot;01220000000LWd0&quot;), CONTAINS( Opportunity.RecordTypeId,&quot;01220000000LWrp&quot;)), ISCHANGED( Line_Type__c), PRIORVALUE(Line_Type__c) &lt;&gt; &quot;New Service&quot;, TEXT(Line_Type__c)=&quot;New Service&quot;)</errorConditionFormula>
        <errorDisplayField>Line_Type__c</errorDisplayField>
        <errorMessage>For Line Types other than New Service (Upgrade, Downgrade, Price Change,...) change to New Service is not allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Line_Type_Restriction_for_Modification</fullName>
        <active>true</active>
        <errorConditionFormula>(ISCHANGED( Line_Type__c ) &amp;&amp; Assigned_to_Order__c = true &amp;&amp; 

(ISPICKVAL( Line_Type__c, &quot;Upgrade&quot;) || 
ISPICKVAL( Line_Type__c, &quot;Downgrade&quot;)|| 
ISPICKVAL( Line_Type__c, &quot;Price Change&quot;)|| 
ISPICKVAL( Line_Type__c, &quot;Technical Change&quot;)) &amp;&amp; 

(!ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Upgrade&quot;) &amp;&amp; 
!ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Downgrade&quot; )&amp;&amp; 
!ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Price Change&quot;)&amp;&amp; 
!ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Technical Change&quot;))) 

||(ISCHANGED( Line_Type__c ) &amp;&amp; Assigned_to_Order__c = true &amp;&amp; 

(!ISPICKVAL( Line_Type__c, &quot;Upgrade&quot;) &amp;&amp; 
!ISPICKVAL( Line_Type__c, &quot;Downgrade&quot;)&amp;&amp; 
!ISPICKVAL( Line_Type__c, &quot;Price Change&quot;)&amp;&amp; 
!ISPICKVAL( Line_Type__c, &quot;Technical Change&quot;)) &amp;&amp; 

(!ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Upgrade&quot;) &amp;&amp; 
!ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Downgrade&quot; )&amp;&amp; 
!ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Price Change&quot;)&amp;&amp; 
!ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Technical Change&quot;))) 

||(ISCHANGED( Line_Type__c ) &amp;&amp; Assigned_to_Order__c = true &amp;&amp; 

(!ISPICKVAL( Line_Type__c, &quot;Upgrade&quot;) &amp;&amp; 
!ISPICKVAL( Line_Type__c, &quot;Downgrade&quot;)&amp;&amp; 
!ISPICKVAL( Line_Type__c, &quot;Price Change&quot;)&amp;&amp; 
!ISPICKVAL( Line_Type__c, &quot;Technical Change&quot;)) &amp;&amp; 

(ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Upgrade&quot;) || 
ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Downgrade&quot; )|| 
ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Price Change&quot;)|| 
ISPICKVAL(PRIORVALUE(Line_Type__c),&quot;Technical Change&quot;)))</errorConditionFormula>
        <errorDisplayField>Line_Type__c</errorDisplayField>
        <errorMessage>Only change between Upgrade, Downgrade, Price Change, Technical Change is allowed!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Line_Type_vs_Opp_Type_Details</fullName>
        <active>false</active>
        <description>Line Type has to correspond to Opportunities Type Details field</description>
        <errorConditionFormula>AND(NOT( $User.FirstName =&quot;System&quot;&amp;&amp; $User.LastName=&quot;Admin&quot; ),NOT(ISNEW()), ISCHANGED(Line_Type__c),
OR(
AND(TEXT(Opportunity.Type_Details__c)= &quot;New Service&quot;, NOT(TEXT(Line_Type__c) =&quot;New Service&quot;)),
AND(TEXT(Opportunity.Type_Details__c) =&quot;Service Management&quot;, NOT(TEXT(Line_Type__c ) =&quot;Upgrade&quot; || TEXT(Line_Type__c ) =&quot;Downgrade&quot;|| TEXT(Line_Type__c )=&quot;Price Change&quot;|| TEXT(Line_Type__c ) =&quot;Suspend&quot;|| TEXT(Line_Type__c ) =&quot;Release&quot;|| TEXT(Line_Type__c )  =&quot;Disconnection&quot;|| TEXT(Line_Type__c )  =&quot;Technical Change&quot;)),
AND(TEXT(Opportunity.Type_Details__c)=&quot;Renewal&quot;, NOT(TEXT(Line_Type__c )  =&quot;Upgrade&quot; || TEXT(Line_Type__c )  =&quot;Downgrade&quot;|| TEXT(Line_Type__c )  =&quot;Price Change&quot;))
))</errorConditionFormula>
        <errorDisplayField>Line_Type__c</errorDisplayField>
        <errorMessage>Line Type has to correspond to Opportunities Type Details!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLD_Values_change</fullName>
        <active>true</active>
        <description>Old MRP Value, Old Monthly Usage and Old Minimal Monthly Usage cannot be changed or other than zero for New Service Opportunity</description>
        <errorConditionFormula>And( 
ISPICKVAL(Line_Type__c, &apos;New Service&apos;), 
OR( 
AND(Old_Monthly_Recurring_Price__c &lt;&gt; 0,not(ISNULL(Old_Monthly_Recurring_Price__c))), 
AND(Old_Minimal_Monthly_Usage__c &lt;&gt; 0,not(ISNULL(Old_Minimal_Monthly_Usage__c))), 
AND(Old_Monthly_Usage__c &lt;&gt; 0,not(ISNULL(Old_Monthly_Usage__c))) 
) 
)</errorConditionFormula>
        <errorMessage>For the Line Type New Service the Old Monthly Recuring Price, Old Monthly Usage and Old Minimal Monthly Usage cannot be other than 0 or empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Product_Distance_Zone</fullName>
        <active>true</active>
        <description>Distance Zone has to correspond to value of Product&apos;s checkboxes: National and International.</description>
        <errorConditionFormula>AND (Product2.International__c = true || Product2.National__c = true,
(ISPICKVAL(Distance_Zone__c, &apos;--None--&apos;)  &amp;&amp;  (Product2.International__c = true  || Product2.National__c = true || Product2.International__c = true &amp;&amp; Product2.National__c = true)) ||
(ISPICKVAL(Distance_Zone__c, &apos;National&apos;)  &amp;&amp; Product2.National__c = false) ||
(ISPICKVAL(Distance_Zone__c, &apos;International&apos;) &amp;&amp; Product2.International__c = false))</errorConditionFormula>
        <errorDisplayField>Distance_Zone__c</errorDisplayField>
        <errorMessage>Sorry, but this value is not allowed for this product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Required</fullName>
        <active>false</active>
        <description>Reason is required for line type Disconnection and Suspend</description>
        <errorConditionFormula>or(
and(ISPICKVAL(Reason__c, &apos;&apos;), ISPICKVAL( Line_Type__c ,&apos;Disconnection&apos;)),
and(ISPICKVAL(Reason__c, &apos;&apos;), ISPICKVAL( Line_Type__c ,&apos;Suspend&apos;))
)</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>The Reason is required for this line type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Renewal_only_with_Up_Dowgrade_PriceChang</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR (CONTAINS( Opportunity.RecordTypeId,&quot;01220000000LWd0&quot;), CONTAINS( Opportunity.RecordTypeId,&quot;01220000000LWrp&quot;)), OR(ISCHANGED( Line_Type__c), ISCHANGED( Renewal__c )),  Renewal__c = true,  ( TEXT(Line_Type__c) &lt;&gt; &quot;Upgrade&quot; &amp;&amp; TEXT(Line_Type__c) &lt;&gt; &quot;Downgrade&quot;&amp;&amp; TEXT(Line_Type__c) &lt;&gt; &quot;Price Change&quot;))</errorConditionFormula>
        <errorDisplayField>Line_Type__c</errorDisplayField>
        <errorMessage>Renewal is allowed only with Upgrade, Downgrade or Price change</errorMessage>
    </validationRules>
