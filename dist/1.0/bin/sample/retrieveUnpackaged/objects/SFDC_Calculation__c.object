    <webLinks>
        <fullName>Accept_for_Realization</fullName>
        <availability>online</availability>
        <description>Accepts Calculation Version and assigns it to Order Lines</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Accept for Realization</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 

var status = &quot;{!SFDC_Calculation__c.Calculation_Status__c}&quot;; 
var calcID = &quot;{!SFDC_Calculation__c.Id}&quot;; 

if(status == &quot;Approved financial terms&quot; || status == &quot;Approved financial terms by CE Management&quot; || status == &quot;Approved financial terms by CEO&quot;){ 
if(confirm(&quot;This will change status of this Calculation and assign it to Opportunity.\nDo you want to continue?&quot;)){ 
var result = sforce.apex.execute(&quot;CalculationMethods&quot;,&quot;acceptForRealization&quot;, {calculationId:calcID}); 
if(result[0] == &apos;OK&apos;){ 
alert(result[1]); 
location.reload(true); 
} else { 
alert(result[1]); 
} 
} 
} else { 
alert(&quot;Only Approved Calculation can be accepted for realization &quot;); 
}</url>
    </webLinks>
