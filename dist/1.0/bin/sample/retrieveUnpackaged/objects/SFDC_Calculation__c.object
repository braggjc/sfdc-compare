    <webLinks>
        <fullName>Select_as_Main_Calculation</fullName>
        <availability>online</availability>
        <description>Selects Calculation as Main one</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Select as Main Calculation</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var records = {!GETRECORDIDS( $ObjectType.SFDC_Calculation__c )};
var OpportunityId = &quot;{! Opportunity.Id}&quot;; 

if (records.length != 1)
   alert(&apos;Exactly one Calculation can be marked as Selected.&apos;);
else{ 

   var soql_result = sforce.connection.query(&quot;select Id, Calculation_Status__c from SFDC_Calculation__c where Id = &apos;&quot; + records [0] + &quot;&apos;&quot;);
   var calculations = soql_result.getArray(&apos;records&apos;);  

   var status = calculations[0].Calculation_Status__c;
   var calcID = calculations[0].Id;

   if(status == &quot;Approved financial terms&quot; || status == &quot;Approved financial terms by CE Management&quot; || status == &quot;Approved financial terms by CEO&quot;){
      if(confirm(&quot;This will select the Calculation as the main one for this Opportunity.\nDo you want to continue?&quot;)){
         var result = sforce.apex.execute(&quot;CalculationMethods&quot;,&quot;acceptForRealization&quot;, {calculationId:calcID});
         if(result[0] == &apos;OK&apos;){
            alert(result[1]);
            location.reload(true);
         } else {
            alert(result[1]);
         }
      }
   } else {
      alert(&quot;Only Approved Calculation can be selected&quot;);
   }
}</url>
    </webLinks>
