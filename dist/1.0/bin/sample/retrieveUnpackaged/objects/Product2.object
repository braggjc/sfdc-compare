    <validationRules>
        <fullName>From_Date_Check</fullName>
        <active>true</active>
        <description>Checks From Date against To Date</description>
        <errorConditionFormula>IF(AND(IF(ISNULL(To_Date__c), false, true), From_Date__c &gt; To_Date__c), true, false)</errorConditionFormula>
        <errorDisplayField>From_Date__c</errorDisplayField>
        <errorMessage>From Date can not be a later date than To Date!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>To_Date_Check</fullName>
        <active>true</active>
        <description>Checks To Date against From Date</description>
        <errorConditionFormula>IF(AND(IF(ISNULL(From_Date__c), false, true), From_Date__c &gt; To_Date__c), true, false)</errorConditionFormula>
        <errorDisplayField>To_Date__c</errorDisplayField>
        <errorMessage>To Date can not be an earlier date than From Date!</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Assign_Prices</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Assign Prices</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 

location.href=&apos;/apex/RelatePricingItem?bid={!Product2.Id}&apos;;</url>
    </webLinks>
    <webLinks>
        <fullName>Configure_Bundle</fullName>
        <availability>online</availability>
        <description>Opens page for Offer Structure Configuration (Bundles and Components)</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Configure Bundle</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 

if({!INCLUDES( Product2.GTS_Location__c , &apos;GTS Slovakia&apos;)}){
location.href=&apos;/apex/RelateBundleComponent?id={!Product2.Id}&apos;;
//location.reload(true);
} else {
alert(&apos;Only Slovakian products can be configured.&apos;);
}</url>
    </webLinks>
    <webLinks>
        <fullName>Configure_Offer_Structure</fullName>
        <availability>online</availability>
        <description>Opens page for Offer Structure Configuration</description>
        <displayType>button</displayType>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>Configure Offer Structure</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <url>/apex/Relate_Services?id={!Product2.Id}</url>
    </webLinks>
    <webLinks>
        <fullName>Synchronize</fullName>
        <availability>online</availability>
        <description>Sends Bundle structure to Extranet and Billing System</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Synchronize</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 

var prodId = &apos;{!Product2.Id}&apos;; 

var synch = &apos;{!Product2.Synchronized__c}&apos;; 

var a = &apos;false&apos;; 

if(synch == 0) { 
var resultString = sforce.apex.execute(&quot;Bundle_Synchronization_SK&quot;,&quot;Synchronize_Bundle&quot;, {bundleId: prodId, test: a}); 
alert(resultString); 
location.reload(true); 
} 

if(synch == 1){ 
alert(&apos;Definition is synchronized.&apos;); 
}</url>
    </webLinks>
