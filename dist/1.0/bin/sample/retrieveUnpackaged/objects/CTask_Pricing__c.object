        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/apex.js&quot;)}


var arr = window.parent.location.href.split(&quot;/&quot;);
var id = arr[3];

var only_id_arr = id.split(&quot;?&quot;);
var only_id = only_id_arr[0];
sforce.connection.sessionId = &apos;{!$Api.Session_ID}&apos;;
var tmpquery = sforce.connection.query(&quot;select OwnerId From CTask__c where Id =&apos;&quot;+ only_id +&quot;&apos; limit 1&quot;);
var tmp = tmpquery.getArray(&quot;records&quot;);

var user = &quot;{!$User.Id}&quot;;
var OwnerIDstring = tmp[0].OwnerId;

if (OwnerIDstring != null){OwnerIDstring = OwnerIDstring.substring(0,15);} 
if (OwnerIDstring ==user)
{
window.open(&quot;https://emea.salesforce.com/apex/EditTaskPricing?id=&quot; + only_id, &quot;Task_Pricing&quot;, &quot;menubar=no,toolbar=no,dependent=yes,scrollbars=yes,width=900,height=480&quot;);
} else {
alert(&apos;Only user which is owner of the Task can change Pricings&apos;);
}</url>
