        <fullName>Assign_Prices</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Assign Prices</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var component = &apos;{!Component__c.Id}&apos;;

var result = sforce.apex.execute(&quot;PC_Webservices_SK&quot;,&quot;CheckComponentFreezability&quot;, {compId: component});

if (result == &apos;OK&apos;) {
location.href=&apos;/apex/RelatePricingItem?cid={!Component__c.Id}&apos;;
} else {
alert(result);
}</url>
    </webLinks>
