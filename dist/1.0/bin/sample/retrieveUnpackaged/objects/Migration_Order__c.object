    <webLinks>
        <fullName>Configure</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Configure</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var morderstatus = &apos;{!Migration_Order__c.Status__c}&apos;;
var morderid = &apos;{!Migration_Order__c.Id}&apos;;

if(morderstatus == &quot;New&quot;) {
alert(&apos;Initial target configuration is not ready yet. First submit the migration order.&apos;);
} else if (morderstatus == &quot;Submitted&quot; || morderstatus == &quot;Preparation failed&quot;) {
alert(&apos;Initial Target configuration is not ready yet. The preparation is in progress. Please try again later.&apos;);
} else if (morderstatus == &quot;Provisioning started&quot; || morderstatus == &quot;Awaiting provisioning&quot; ) {
alert(&apos;The provisioning is already in progress and the configuration cannot be changed.&apos;);
} else if (morderstatus == &quot;Completed&quot;) {
alert(&apos;Migration order has already been completed and configuration cannot be changed.&apos;);
} else if (morderstatus == &quot;Prepared&quot;) {
var result1 = sforce.connection.query(&quot;Select Installation_Order__r.Id From Migration_Order__c Where Id = &apos;&quot; + morderid + &quot;&apos;&quot;);
var morder = result1.getArray(&apos;records&apos;);
var installationorderid = morder[0].Installation_Order__r.Id;
var result = sforce.connection.query(&quot;Select Id, Status__c, OrderLookup__r.OrderType__c, Line_Type__c From OrderLine__c Where OrderLookup__c = &apos;&quot; + installationorderid + &quot;&apos;&quot;);
var orderlines = result.getArray(&apos;records&apos;);
window.location.href = &quot;/apex/OrderAtrributesModification?olid=&quot; + orderlines[0].Id;
}</url>
    </webLinks>
    <webLinks>
        <fullName>Start_Provisioning</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Start Provisioning</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var ordStatus = &apos;{!Migration_Order__c.Status__c}&apos;;
var ordId = &apos;{!Migration_Order__c.Id}&apos;;
var proceed_with_submit = false;

var validation_result = sforce.apex.execute(&quot;MigrationOrderMethods&quot;,&quot;startProvisionigValidation&quot;, {ordId:ordId, ordStatus:ordStatus });

if(validation_result != null){
if(validation_result[0] == &apos;OK&apos;){
proceed_with_submit = true;
} else if(validation_result[0] == &apos;QA&apos;){
if(confirm(validation_result[1])){
proceed_with_submit = true;
}
} else if(validation_result[0] == &apos;ERR&apos;){
alert(validation_result[1]);
}
}

if(proceed_with_submit == true){
var result = sforce.apex.execute(&quot;MigrationOrderMethods&quot;,&quot;startProvisionig&quot;, {ordId:ordId, ordStatus:ordStatus });
if(result[0] == &apos;OK&apos;){
alert(result[1]);
location.reload(true);
} else if(result[0] == &apos;TRNSFR&apos;){
window.top.location.href = &quot;https://&quot; + window.top.location.host + result[1];
} else {
alert(result[1]);
}
}</url>
    </webLinks>
