    <validationRules>
        <fullName>Reason_Lost_Comment_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND (ISPICKVAL(Stage__c , &quot;Closed Lost&quot;), Reason_Lost_Comments__c =null )</errorConditionFormula>
        <errorDisplayField>Reason_Lost_Comments__c</errorDisplayField>
        <errorMessage>If The Stage equals &quot;Closed Lost&quot; the &quot;Reason Lost Comments&quot; is a required field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Lost_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( ISPICKVAL(Stage__c , &quot;Closed Lost&quot;), ISPICKVAL(Reason_Lost__c, &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Reason_Lost__c</errorDisplayField>
        <errorMessage>If the stage equals &quot;Closed Lost&quot;, then the Reason Lost field is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_of_Percentages_RETAIL</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISPICKVAL(Stage__c, &quot;Suspect&quot;), (Probability__c &lt;0) || (Probability__c &gt; 0.10),
IF(ISPICKVAL(Stage__c, &quot;Prospect&quot;), (Probability__c &lt; 0) || (Probability__c &gt; 0.30),
IF(ISPICKVAL(Stage__c, &quot;Proposal&quot;), (Probability__c &lt;0.10) || (Probability__c &gt; 0.50),
IF(ISPICKVAL(Stage__c, &quot;Negotiation&quot;), (Probability__c &lt; 0.20) || (Probability__c &gt; 0.90),
IF(ISPICKVAL(Stage__c, &quot;Contract Signature&quot;), (Probability__c &lt;0.30) || (Probability__c &gt; 1),
false)))))</errorConditionFormula>
        <errorDisplayField>Probability__c</errorDisplayField>
        <errorMessage>Wrong Probability Value for selected Stage Please change</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>max_100percent</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
   CS_Manager_Involvement__c &gt; 1.0,
   CS_Manager_Involvement__c &lt; 0.0
)</errorConditionFormula>
        <errorDisplayField>CS_Manager_Involvement__c</errorDisplayField>
        <errorMessage>The value must be between 0-100%.</errorMessage>
    </validationRules>
