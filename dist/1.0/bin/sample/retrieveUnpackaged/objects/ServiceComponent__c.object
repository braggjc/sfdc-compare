    <validationRules>
        <fullName>Service_Type_Validation</fullName>
        <active>false</active>
        <description>Verifies if selected Product Component is Related to Correct Product</description>
        <errorConditionFormula>AND((ServiceType__r.Product__c != ServiceBundle__r.BundleType__c),  NOT(ISBLANK(ServiceType__c)))</errorConditionFormula>
        <errorDisplayField>ServiceType__c</errorDisplayField>
        <errorMessage>Selected Service type is incorrect for related Bundle</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Service_Type_or_Component_Validation</fullName>
        <active>true</active>
        <description>Validates if either Service Type or Component has been selected</description>
        <errorConditionFormula>ISBLANK(ServiceType__c) &amp;&amp; ISBLANK(Component__c)</errorConditionFormula>
        <errorMessage>Service Type or Component is Required</errorMessage>
    </validationRules>
