    <label>Provisioning Plan Task</label>
    <nameField>
        <displayFormat>PPT{000000000}</displayFormat>
        <label>Name</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Provisioning Plan Tasks</pluralLabel>
    <searchLayouts>
        <lookupDialogsAdditionalFields>Task_Name__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>Task_Sequence__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>Initial_Status__c</lookupDialogsAdditionalFields>
    </searchLayouts>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Assignee_or_Queue_Required</fullName>
        <active>true</active>
        <description>Either Assignee or Queue is required for Task definition</description>
        <errorConditionFormula>OR( 
AND( 
NOT(ISBLANK( Task_Assignee__c )), 
NOT(ISPICKVAL( QueueName__c, &apos;&apos;)) 
) 
)</errorConditionFormula>
        <errorMessage>Either Assignee or Queue is required for Task definition</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Component_Required_for_OSS_Task</fullName>
        <active>false</active>
        <description>Verifies if component was provided for OSS Task</description>
        <errorConditionFormula>AND( ISPICKVAL( Task_Type__c , &apos;OSS Provisioning&apos;) , ISBLANK( Component__c ))</errorConditionFormula>
        <errorDisplayField>Component__c</errorDisplayField>
        <errorMessage>Component is required for OSS Provisioning Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Costing_Task_only_on_instance</fullName>
        <active>false</active>
        <description>Relation to costing task can be filled in  only on task instance.</description>
        <errorConditionFormula>NOT(Provisioning_Plan__r.Instance__c)  &amp;&amp; NOT(ISBLANK( Costing_Task__c ))</errorConditionFormula>
        <errorDisplayField>Costing_Task__c</errorDisplayField>
        <errorMessage>Relation to costing task can be filled in  only on task instance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delay_Calculation</fullName>
        <active>false</active>
        <description>If Delay is checked, Delay Calculation must be filled</description>
        <errorConditionFormula>AND( Delay__c = true, 
NOT(
OR(
ISPICKVAL(Delay_Calculation__c,&apos;Hours&apos;), ISPICKVAL(Delay_Calculation__c,&apos;Business Hours&apos;)
)
)
)</errorConditionFormula>
        <errorDisplayField>Delay_Calculation__c</errorDisplayField>
        <errorMessage>Delay Calculation is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delay_Hours</fullName>
        <active>false</active>
        <description>If Delay is checked, Delay Hours must be filled</description>
        <errorConditionFormula>AND( Delay__c = true, OR(ISBLANK(Delay_Hours__c),Delay_Hours__c = 0, Delay_Hours__c = null))</errorConditionFormula>
        <errorDisplayField>Delay_Hours__c</errorDisplayField>
        <errorMessage>Delay Hours is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_instance_only</fullName>
        <active>true</active>
        <description>Due Date can be set on instance only.</description>
        <errorConditionFormula>AND(Provisioning_Plan__r.Instance__c = false,NOT(ISBLANK( Due_Date__c)))</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>Due Date can be set on Provisioning Plan instance only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_past</fullName>
        <active>true</active>
        <description>Due Date can not be set in past.</description>
        <errorConditionFormula>Due_Date__c &lt; NOW()</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>Due Date can not be set in past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GP_for_Automatic_Provisioning_Required</fullName>
        <active>true</active>
        <description>Verifies that for Automatic Provisioning Tasks GP vas provided</description>
        <errorConditionFormula>AND( ISPICKVAL( Task_Type__c , &apos;Automatic Provisioning&apos;),  ISBLANK( Generic_Product__c ) )</errorConditionFormula>
        <errorDisplayField>Generic_Product__c</errorDisplayField>
        <errorMessage>Generic Product is Required for Automatic Provisioning Tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GP_for_Yellowpages_Required</fullName>
        <active>false</active>
        <description>Verifies that for Yellowpages (PL) Tasks GP vas provided</description>
        <errorConditionFormula>AND( ISPICKVAL( Task_Type__c , &apos;Yellowpages (PL)&apos;), ISBLANK( Generic_Product__c ) )</errorConditionFormula>
        <errorDisplayField>Generic_Product__c</errorDisplayField>
        <errorMessage>Generic Product is required for Yellowpages automatic ta</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OSS_Task_not_in_Task_Plan</fullName>
        <active>false</active>
        <description>OSS Task can not be added to Task Plan</description>
        <errorConditionFormula>AND(Provisioning_Plan__r.RecordTypeId = &apos;01220000000Lgac&apos;, ISPICKVAL(Task_Type__c,&apos;OSS Provisioning&apos;))</errorConditionFormula>
        <errorDisplayField>Task_Type__c</errorDisplayField>
        <errorMessage>OSS Provisioning task can not be added to Task Plan</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_at_Datetime</fullName>
        <active>false</active>
        <description>If &quot;Start at&quot; is checked, &quot;Start at Datetime&quot; must be filled.</description>
        <errorConditionFormula>AND( Provisioning_Plan__r.Instance__c = true,Start_at__c = true, ISBLANK(Start_at_Datetime__c ))</errorConditionFormula>
        <errorDisplayField>Start_at_Datetime__c</errorDisplayField>
        <errorMessage>Start at Date time is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_at_Instance</fullName>
        <active>false</active>
        <description>&quot;Start at&quot; can be set only for Provisioning Plan instance.</description>
        <errorConditionFormula>AND(Provisioning_Plan__r.Instance__c = false, OR( Start_at__c = true, NOT(ISBLANK(Start_at_Datetime__c))))</errorConditionFormula>
        <errorMessage>Start Datetime of task can be set only on Provisioning Plan instance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_at_or_Delay</fullName>
        <active>false</active>
        <description>Only one option is vaid - either Start at or Delay</description>
        <errorConditionFormula>AND(Provisioning_Plan__r.Instance__c = true,Delay__c = true,Start_at__c = true)</errorConditionFormula>
        <errorMessage>Only one option can be selected - either &quot;Delay&quot; or &quot;Start at&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_at_past</fullName>
        <active>false</active>
        <description>&quot;Start at&quot; can not be set in past</description>
        <errorConditionFormula>AND(Provisioning_Plan__r.Instance__c = true,Start_at_Datetime__c &lt;= NOW())</errorConditionFormula>
        <errorDisplayField>Start_at_Datetime__c</errorDisplayField>
        <errorMessage>Start at Datetime must be in future</errorMessage>
    </validationRules>
</CustomObject>
