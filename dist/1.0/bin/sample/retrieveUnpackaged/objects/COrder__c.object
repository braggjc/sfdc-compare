        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var ordStatus = &apos;{!COrder__c.Status__c}&apos;;
var ordType = &apos;{!COrder__c.OrderType__c}&apos;;
var ordId = &apos;{!COrder__c.Id}&apos;;
var proceed_with_cancel = false;

var validation_result = sforce.apex.execute(&quot;IntegrationOrder&quot;,&quot;validateCancel&quot;, {ordId:ordId, ordType:ordType, ordStatus:ordStatus });

if(validation_result != null){
if(validation_result[0] == &apos;OK&apos;){
proceed_with_cancel = true;
} else if(validation_result[0] == &apos;QA&apos;){
if(confirm(validation_result[1])){
proceed_with_cancel = true;
}
} else if(validation_result[0] == &apos;ERR&apos;){
alert(validation_result[1]);
}
}

if(proceed_with_cancel == true){
var result = sforce.apex.execute(&quot;IntegrationOrder&quot;,&quot;processCancel&quot;, {ordId:ordId, ordType:ordType, ordStatus:ordStatus });
if(result[0] == &apos;OK&apos;){
alert(result[1]);
location.reload(true);
} else if(result[0] == &apos;TRNSFR&apos;){
window.top.location.href = &quot;https://&quot; + window.top.location.host + result[1];
} else {
alert(result[1]);
}
}</url>
