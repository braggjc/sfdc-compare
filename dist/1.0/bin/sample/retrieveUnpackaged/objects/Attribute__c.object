    <validationRules>
        <fullName>AttributeTagValidation</fullName>
        <active>true</active>
        <description>Checks format of Attribute Tag (first and last character, spaces)</description>
        <errorConditionFormula>AND(
NOT( ISBLANK( Attribute_Tag__c ) ),
OR(
NOT(LEFT(Attribute_Tag__c, 1) = &quot;#&quot;),
NOT(RIGHT(Attribute_Tag__c, 1) = &quot;#&quot;),
FIND(&quot; &quot;,Attribute_Tag__c) &gt; 0,
LEN(Attribute_Tag__c) &lt; 3
)
)</errorConditionFormula>
        <errorDisplayField>Attribute_Tag__c</errorDisplayField>
        <errorMessage>Incorrect format (must start and end with &apos;#&apos; and contain no spaces)</errorMessage>
    </validationRules>
