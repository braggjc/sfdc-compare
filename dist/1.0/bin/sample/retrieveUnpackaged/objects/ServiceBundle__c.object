    <webLinks>
        <fullName>Technical_Change</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Technical Change</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 

var sbId = &apos;{!ServiceBundle__c.Id}&apos;; 
var validation_result = sforce.apex.execute(&quot;ModificationOrderMethods&quot;,&quot;checkIfServiceBundleHasAnyActiveOrderLine&quot;, {sbId:sbId}); 
if(validation_result == &apos;true&apos;){ 
alert(&apos;There are already pending OrderLines for this Service Bundle. You can not create modification line until they are finished.&apos;); 
}else{ 
if (confirm(&apos;This will create a new Technical Change Order. Do you want to continue?&apos;)) { 
var callout_result = sforce.apex.execute(&quot;ModificationOrderMethods&quot;,&quot;createTechnicalOrder&quot;, {serviceBundleId:sbId}); 
if(callout_result[0] == &apos;0&apos;){ 
alert(&apos;The new order has been successfully created.&apos;); 
window.top.location.href = &quot;https://&quot; + window.top.location.host + &quot;/&quot; +callout_result[1]; 
} else { 
alert(callout_result[1]); 
} 
} 
}</url>
