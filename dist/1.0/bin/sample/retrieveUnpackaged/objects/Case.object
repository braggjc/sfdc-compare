    <validationRules>
        <fullName>AssignMeBeforeClose</fullName>
        <active>true</active>
        <description>The IS Case have to be owned by user (not Queue) for closing it</description>
        <errorConditionFormula>AND($User.Profile_Name__c !=&apos;System Administrator&apos;,RecordTypeId = &apos;01220000000JKR1&apos; , OwnerId = &apos;00G20000001Gk2T&apos;, OR(ISPICKVAL( Status , &apos;closed - Resolved&apos;),ISPICKVAL( Status , &apos;closed - Irresolvable&apos;),ISPICKVAL( Status , &apos;closed - Unresolvable&apos;)) )</errorConditionFormula>
        <errorMessage>The case must be assigned to the user before closing it. Please use the &quot;Assign me&quot; button.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BlockRelationshipCreationForClosedTT</fullName>
        <active>true</active>
        <errorConditionFormula>if(AND(ISNEW(),Parent.IsClosed), true, false)</errorConditionFormula>
        <errorMessage>You can not assign case, or modify it, when the parent is closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BlockRelationshipToChildCase</fullName>
        <active>true</active>
        <description>Sprint 2 requirement;</description>
        <errorConditionFormula>if(AND(ISNEW(), Parent.ParentId!=null),true,false)</errorConditionFormula>
        <errorMessage>You can not create a subcase from a child ticket.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CaseChangeToNew</fullName>
        <active>false</active>
        <description>Block illegal case status transition;</description>
        <errorConditionFormula>IF(AND( ISPICKVAL(Status, &apos;New&apos; ), ISCHANGED(Status)), true, false)</errorConditionFormula>
        <errorMessage>After Case processing start, it can not return to NEW status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CaseDesciptionChange</fullName>
        <active>true</active>
        <description>VR blocks the possibility of changing the case description.</description>
        <errorConditionFormula>AND(ISCHANGED(Description),CONTAINS($RecordType.DeveloperName,&apos;Colorado&apos;),  NOT(ISBLANK(PRIORVALUE(Description))))</errorConditionFormula>
        <errorMessage>Description field can not be modified, after its creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CasePriorityChange</fullName>
        <active>true</active>
        <description>CARE_CZ_023 Change of TT priority</description>
        <errorConditionFormula>IF( AND($User.Profile_Name__c !=&apos;System Administrator&apos;,$User.Id != OwnerId, NOT(CONTAINS($User.Username, &apos;admin@gtsce.com&apos;)), ISCHANGED( Priority )),true, false)</errorConditionFormula>
        <errorMessage>You are not allowed to modify the Case Priority</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CaseReopening</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IF(PRIORVALUE(IsClosed),true,false)
,AND(If(NOT(ISPICKVAL(Status, &apos;Closed&apos;)) ,true, false), If(NOT(ISPICKVAL(Status, &apos;Closed - Resolved&apos;)),true, false), If(NOT(ISPICKVAL(Status, &apos;Closed - Irresolvable&apos;)),true,false)),if(NOW()&gt; ClosedDate+31, true, false ))</errorConditionFormula>
        <errorMessage>Case can be reopened only till 31 days after its closing!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CaseTypeCheck</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(NOT(ISCHANGED( Synchronization_in_Progress__c)),ISBLANK(SuppliedEmail), OR(AND($RecordType.Name = &apos;Info&apos;, NOT(OR(ISPICKVAL(Type,&quot;General&quot;),
ISPICKVAL(Type,&quot;Technical&quot;),
ISPICKVAL(Type,&quot;Financial&quot;),
ISPICKVAL(Type,&quot;Welcome Call&quot;),
ISPICKVAL(Type,&quot;Other&quot;)
)
)
),
 AND(
$RecordType.Name = &apos;Service Management&apos;, NOT(
             OR(
                 ISPICKVAL(Type,&quot;Account Transfer&quot;),
                 ISPICKVAL(Type,&quot;Activation&quot;),
                 ISPICKVAL(Type,&quot;Change of Address&quot;),
                 ISPICKVAL(Type,&quot;Modification&quot;),
                 ISPICKVAL(Type,&quot;Termination&quot;),
                 ISPICKVAL(Type,&quot;Other&quot;)
             )
)
       ),
AND(
          $RecordType.Name = &apos;Churn&apos;, NOT(
             OR(
                 ISPICKVAL(Type,&quot;Notice&quot;),
                 ISPICKVAL(Type,&quot;Info&quot;),
                 ISPICKVAL(Type,&quot;Service Migration&quot;),
                 ISPICKVAL(Type,&quot;Service Transfer&quot;),
                 ISPICKVAL(Type,&quot;Wholesale&quot;),
                 ISPICKVAL(Type,&quot;Contract Expiration&quot;)
             )
) 
      ),
 AND(
          $RecordType.Name = &apos;Technical&apos;, NOT(
             OR(
                 ISPICKVAL(Type,&quot;Mass Outage&quot;),
                 ISPICKVAL(Type,&quot;QoS&quot;),
                 ISPICKVAL(Type,&quot;Single Outage&quot;),
                 ISPICKVAL(Type,&quot;Planned Outage&quot;),
                 ISPICKVAL(Type,&quot;Other&quot;)
             )
) 
      ),
 AND(
          $RecordType.Name = &apos;Network&apos;, NOT(
             OR(
                 ISPICKVAL(Type,&quot;Mass Outage&quot;),
                 ISPICKVAL(Type,&quot;Planned Outage&quot;),
                 ISPICKVAL(Type,&quot;Other&quot;)
             )
) 
      ),
AND(
          $RecordType.Name = &apos;Financial&apos;, NOT(
             OR(
                 ISPICKVAL(Type,&quot;Credit Note&quot;),
                 ISPICKVAL(Type,&quot;Direct Debit&quot;),
                 ISPICKVAL(Type,&quot;Invoice Duplicate&quot;),
                 ISPICKVAL(Type,&quot;SIPO&quot;),
                 ISPICKVAL(Type,&quot;Other&quot;),
                 ISPICKVAL(Type,&quot;Resend Invoice&quot;),
                 ISPICKVAL(Type,&quot;Invoice Complaint&quot;),
                 ISPICKVAL(Type,&quot;Info Request&quot;),
                 ISPICKVAL(Type,&quot;Payment Problem&quot;),
                 ISPICKVAL(Type,&quot;Bad Header Data&quot;)
             )
) 
      ),
 AND(
          $RecordType.Name = &apos;Sales&apos;, NOT(
             OR(
                 ISPICKVAL(Type,&quot;Demand&quot;)
             )
) 
      ),
       AND(
          $RecordType.Name = &apos;Complaint&apos;, NOT(
             OR(
                 ISPICKVAL(Type,&quot;Communication / Process&quot;),
                 ISPICKVAL(Type,&quot;Technical&quot;),
ISPICKVAL(Type,&quot;Financial&quot;)
)
)
)
)),true, false)</errorConditionFormula>
        <errorMessage>Please choose correct Case Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CaseTypeRequirement</fullName>
        <active>true</active>
        <errorConditionFormula>if( AND(ISBLANK( SuppliedEmail ),ISPICKVAL( Type , &apos;- Choose -&apos;)), true, false)</errorConditionFormula>
        <errorMessage>Case Type needs to be specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckBeforeClose</fullName>
        <active>true</active>
        <description>Checks whether the required ClosureCode / Summary has been delivered during the case closing attempt;</description>
        <errorConditionFormula>IF(AND(ISPICKVAL(Status, &apos;Closed&apos;),  OR(ISNULL(Summary__c), ISPICKVAL( Closure_Code__c,&apos;&apos;) ) ),true, false)</errorConditionFormula>
        <errorMessage>&quot;Closure Code&quot; and &quot;Summary&quot; field needs to be delivered, before case is closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckBeforeCloseChurn</fullName>
        <active>true</active>
        <description>Checks if the additional output mandatory  information (CARE_CZ_027 Additional output information) are provided, before case is closed;</description>
        <errorConditionFormula>if(AND($RecordType.Name=&apos;Churn&apos;,  TEXT(Status) = &apos;Closed&apos;, OR( ISBLANK(Notice_Received_Date__c),  ISBLANK(Termination_Date__c),  TEXT(Churn_Result__c) = &apos;&apos;,  TEXT(Churn_Reason__c) = &apos;&apos;)),true,false)</errorConditionFormula>
        <errorMessage>To close Churn case, You need to deliver information about: Termination Date, Notice Recieved Date, Churn Result and Churn Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckBeforeCloseComplaint</fullName>
        <active>true</active>
        <description>Checks if the additional output mandatory information (CARE_CZ_027 Additional output information) are provided, before case is closed;</description>
        <errorConditionFormula>if(AND($RecordType.Name=&apos;Complaint&apos;, TEXT(Status) = &apos;Closed&apos;, OR( ISBLANK( Claim_Received_Date__c), ISBLANK( Confirmation_Sent_Date__c), ISBLANK( Statement_Sent_Date__c),  TEXT( Reason__c ) = &apos;&apos;, TEXT(  Responsible_Dept__c ) = &apos;&apos;, TEXT(  Claim_Result__c ) = &apos;&apos;)),true,false)</errorConditionFormula>
        <errorMessage>To close Complaint case, You need to deliver information about: Claim Recieved Date, Confirmation Sent Date, Statement Sent Date, Reason, Responsible Dept. and Claim Result.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckBeforeCloseTechnical</fullName>
        <active>true</active>
        <description>Checks if the additional output mandatory information (CARE_CZ_031) are provided, before case is closed;</description>
        <errorConditionFormula>if(AND($RecordType.Name=&apos;Technical&apos;, TEXT(Status) = &apos;Closed&apos;, ISBLANK( Summary__c )),true,false)</errorConditionFormula>
        <errorMessage>To close Technical case, You need to deliver (Technical) Summary.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CloseCaseJustOnce</fullName>
        <active>true</active>
        <errorConditionFormula>AND($User.Profile_Name__c !=&apos;System Administrator&apos;,RecordTypeId = &apos;01220000000JKR1&apos; ,OR(ISPICKVAL( Priorvalue(Status) , &apos;closed - Resolved&apos;),ISPICKVAL( Priorvalue(Status) , &apos;closed - Irresolvable&apos;),ISPICKVAL(Priorvalue(Status), &apos;closed - Unresolvable&apos;)) )</errorConditionFormula>
        <errorMessage>The case is already closed! It´s not possible to close case more than once.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Postponement_Check</fullName>
        <active>true</active>
        <description>Checks wheter  the postponement reason were set</description>
        <errorConditionFormula>IF(AND(ISBLANK(TEXT( Reason_of_Postponment__c )), ISPICKVAL(Status, &quot;Postponed&quot;) ),true, false)</errorConditionFormula>
        <errorMessage>You need to specify the reason before setting the postponement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Postponement_Check2</fullName>
        <active>true</active>
        <description>Checks whether</description>
        <errorConditionFormula>IF(OR(AND( TEXT(Status) = &apos;Postponed&apos;, 
           ISBLANK( Postpone_Till__c),  
           ISBLANK(TEXT( Postponement_Duration__c))
         ),
      AND(NOT(ISBLANK(TEXT( Reason_of_Postponment__c ))), 
      ISBLANK( Postpone_Till__c),  
      ISBLANK(TEXT( Postponement_Duration__c)))
  ), true, false)</errorConditionFormula>
        <errorMessage>You have to specify Postponement Duration or Postponement Time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RequiredContactData</fullName>
        <active>true</active>
        <description>Delivered during the Sprint 2 Colorado project;</description>
        <errorConditionFormula>AND( 
IF( 
AND( ISBLANK(SuppliedEmail), 
ISBLANK(ContactId), 
OR( 
ISBLANK(Contact_info__c), 
AND( ISBLANK(Contact_s_Email__c), ISBLANK( Contact_Phone_No__c) ) 
), 
NOT( 
OR( 
ISPICKVAL( Origin , &apos;Extranet PL&apos;), 
ISPICKVAL( Origin , &apos;Extranet&apos;), 
ISPICKVAL( Origin , &apos;Extranet SK&apos;) 
