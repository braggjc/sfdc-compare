        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 

function getRandomNum(lbound, ubound) 
{ 
return (Math.floor(Math.random() * (ubound - lbound)) + lbound); 
} 

function getRandomChar() 
{ 
var numberChars = &quot;0123456789&quot;; 
var alphaChars = &quot;abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ&quot;; 
var charSet = numberChars + alphaChars; 

return charSet.charAt(getRandomNum(0, charSet.length)); 
} 

function getPassword(length) 
{ 
var pass = &quot;&quot;; 
length++; 

if (length &gt; 0) pass = pass + getRandomChar(); 

for (var idx = 1; idx &lt; length; ++idx) 
{ 
pass = pass + getRandomChar(); 
} 
return pass; 
} 

function openExtranetForm(url){ 
window.open(url); 
} 

var url = &apos;http://extranet.gtsce.com/web/guest/attachement&apos;; 
var str_password = getPassword(24); 
var oppId = &apos;{!Account.Id}&apos;; 
var create_ref_result = sforce.apex.execute(&quot;ExternalAttachmentMethods&quot;,&quot;createReferenceObject&quot;, {hash:str_password, objectId:oppId, objectType:&apos;Account&apos;}); 

if(create_ref_result != null){ 
if(create_ref_result[0] == &apos;OK&apos;){ 
openExtranetForm(url + &apos;?requestId=&apos; + str_password); 
} else if(validation_result[0] == &apos;REF_ERROR&apos;){ 
alert(&apos;Try again&apos;); 
} 
}</url>
    </webLinks>
