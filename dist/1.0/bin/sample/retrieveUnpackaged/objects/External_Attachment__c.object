var oppId = &apos;{!Lead.Id}&apos;; 
var create_ref_result = sforce.apex.execute(&quot;ExternalAttachmentMethods&quot;,&quot;createReferenceObject&quot;, {hash:str_password, objectId:oppId, objectType:&apos;Lead&apos;}); 

if(create_ref_result != null){ 
if(create_ref_result[0] == &apos;OK&apos;){ 
openExtranetForm(url + &apos;?requestId=&apos; + str_password); 
} else if(validation_result[0] == &apos;REF_ERROR&apos;){ 
alert(&apos;Try again&apos;); 
} 
}</url>
