    <validationRules>
        <fullName>Automatic_tasks</fullName>
        <active>true</active>
        <description>Automatic tasks should not have possibillity to start conditional plans.</description>
        <errorConditionFormula>OR(
ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c,&apos;Update of Billing Products&apos;),
ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c,&apos;Billing Account Creation&apos;),
ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c,&apos;IP Unregistration&apos;),
ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c,&apos;IP Activation&apos;),
ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c,&apos;Phone Number Release&apos;),
ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c,&apos;Phone Number Activation&apos;),
ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c,&apos;Automatic Provisioning (ST)&apos;),
ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c,&apos;Automatic Provisioning&apos;),
ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c,&apos;Update of CSR Database&apos;)
)</errorConditionFormula>
        <errorMessage>Plans for selection can not be set for automatic task.</errorMessage>
    </validationRules>
