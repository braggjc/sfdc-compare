    <validationRules>
        <fullName>Name_not_null</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Name__c )</errorConditionFormula>
        <errorDisplayField>Name__c</errorDisplayField>
        <errorMessage>You must enter a Name value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Line_not_null</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Project_cost__r.IS_Local__c == &apos;Local&apos;, 
     AND(ISBLANK(Order_Line__c), 
               NOT(AND( ISBLANK( Request__c ) , ISBLANK( Task_Pricing__c )  )  ) ) , 
     false)</errorConditionFormula>
        <errorDisplayField>Order_Line__c</errorDisplayField>
        <errorMessage>You must select Order Line</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_line_not_Request</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Project_cost__r.IS_Local__c == &apos;Local&apos;,
NOT( ISBLANK( Request__c ) ),
false)</errorConditionFormula>
        <errorMessage>You must select Order Line not Request. For Local type you can only add or change Order Line.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_line_not_RequestOrTaskPricing</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Project_cost__r.IS_Local__c == &apos;Local&apos;,
NOT( AND ( ISBLANK( Request__c ) , ISBLANK( Task_Pricing__c ) )  ),
false)</errorConditionFormula>
        <errorMessage>You must select Order Line not Request or Task Pricing. For Local type you can only add or change Order Line.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_not_Order_line</fullName>
        <active>false</active>
        <errorConditionFormula>IF( Project_cost__r.IS_Local__c == &apos;IS&apos;, 
     NOT( ISBLANK( Order_Line__c ) ), 
     false)</errorConditionFormula>
        <errorMessage>You must select Request not Order Line. For IS type you can only add or change Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_not_null</fullName>
        <active>false</active>
        <errorConditionFormula>IF( Project_cost__r.IS_Local__c == &apos;IS&apos;, 
     ISBLANK( Request__c ), 
     false)</errorConditionFormula>
        <errorDisplayField>Request__c</errorDisplayField>
        <errorMessage>You must select Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_Pricing_not_Order_line</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Project_cost__r.IS_Local__c == &apos;IS&apos;, 
     NOT( ISBLANK( Order_Line__c ) ), 
     false)</errorConditionFormula>
        <errorMessage>You must select Task Pricing not Order Line. For IS type you can only add or change  Task Pricing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_Pricing_not_null</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Project_cost__r.IS_Local__c == &apos;IS&apos;,
ISBLANK(  Task_Pricing__c  ),
false)</errorConditionFormula>
        <errorDisplayField>Task_Pricing__c</errorDisplayField>
        <errorMessage>You must select Task Pricing</errorMessage>
    </validationRules>
