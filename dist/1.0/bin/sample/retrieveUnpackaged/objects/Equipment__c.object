    <validationRules>
        <fullName>Adding_to_OL_Task</fullName>
        <active>false</active>
        <description>Equipment can be added to &quot;Hardware Request&quot; Task only</description>
        <errorConditionFormula>AND(NOT(ISBLANK( Task__c )),NOT(ISPICKVAL( Task__r.Type__c ,&apos;Hardware Request (PL)&apos;)))</errorConditionFormula>
        <errorMessage>Equipment can be added to &quot;Hardware Request&quot; Task only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Adding_to_Prov_Plan_Task</fullName>
        <active>false</active>
        <description>It&apos;s allowed to add equipment to Provisioning Plan Task type &quot;Hardware Request&quot; only.</description>
        <errorConditionFormula>AND(NOT(ISBLANK(Provisioning_Plan_Task__c)),NOT(ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c,&apos;Hardware Request (PL)&apos;)))</errorConditionFormula>
        <errorMessage>Equipment can be added to Hardware Request task only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Component_selection_1</fullName>
        <active>false</active>
        <description>Selected Order Component has to be related to proper Order Line (via Provisioning Plan Task)</description>
        <errorConditionFormula>AND(NOT(ISBLANK( Order_Component__c)),NOT(ISBLANK( Provisioning_Plan_Task__c )),Order_Component__r.OrderLine__r.Id != Provisioning_Plan_Task__r.Provisioning_Plan__r.Order_Line__r.Id)</errorConditionFormula>
        <errorDisplayField>Order_Component__c</errorDisplayField>
        <errorMessage>Selected Order Component doesn&apos;t belong to the Order Line</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Component_selection_2</fullName>
        <active>false</active>
        <description>Selected Order Component has to be related to proper Order Line (via OL Task)</description>
        <errorConditionFormula>AND(NOT(ISBLANK(Order_Component__c)),NOT(ISBLANK(Task__c)),Order_Component__r.OrderLine__c != Task__r.Order_Line__c)</errorConditionFormula>
        <errorDisplayField>Order_Component__c</errorDisplayField>
        <errorMessage>Selected Order Component doesn&apos;t belong to the Order Line</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Provisioning_Plan_definition</fullName>
        <active>false</active>
        <description>Equipment can not be added to Provisioning Plan Task in Provisioning Plan definition (instance only)</description>
        <errorConditionFormula>Provisioning_Plan_Task__r.Provisioning_Plan__r.Instance__c = false</errorConditionFormula>
        <errorMessage>Equipment data can be added to Provisioning Plan instance only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Service_Component_installation_only</fullName>
        <active>false</active>
        <description>Service Component should be selected for equipment installation only.</description>
        <errorConditionFormula>AND(NOT(ISBLANK(Order_Component__c)),NOT(OR(ISPICKVAL( Action__c, &apos;client installation&apos;),ISPICKVAL( Action__c, &apos;POP installation&apos;))))</errorConditionFormula>
        <errorDisplayField>Order_Component__c</errorDisplayField>
        <errorMessage>Order Component can be selected for installations only.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Create_Logistics_Order</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Logistics Order</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var records = {!GETRECORDIDS( $ObjectType.Equipment__c )};

var result = sforce.apex.execute(&quot;IntegrationLogistics&quot;,&quot;CreateLogisticsOrder&quot;, {records:records});

if(result[0] == &apos;OK&apos;){
alert(result[1]);
location.reload(true);
} else {
alert(result[1]);
}</url>
    </webLinks>
