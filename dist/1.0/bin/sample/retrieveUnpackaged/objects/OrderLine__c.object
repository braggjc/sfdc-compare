    <webLinks>
        <fullName>Calculate_Selected_Order_Lines</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Calculate Selected Order Lines</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var records = {!GETRECORDIDS( $ObjectType.OrderLine__c )};
var OpportunityId = &quot;{! Opportunity.Id}&quot;;
var SalesTypeVerified = &quot;{!Opportunity.Sales_Type_Verified__c}&quot;;
var OpportunityOwnerId = &quot;{! Opportunity.OwnerId}&quot;;
var result;

if (records.length == 0)
alert(&apos;Please select at least one Order Line to Calculate.&apos;);
else{
result=sforce.apex.execute(&quot;SFDCCalculation&quot;,&quot;CalculateOrderLines&quot;, {SalesTypeVerified:SalesTypeVerified,OpportunityId:OpportunityId ,OpportunityOwnerId:OpportunityOwnerId,OrderLinesId:records});

if (result==&apos;OK&apos;)
window.location.href = window.location.href;
else
alert(&apos;OrderLine/OrderLines can not be calculated, due to the fact that: &apos;+result);
}</url>
    </webLinks>
    <webLinks>
        <fullName>CancelOrderLine</fullName>
        <availability>online</availability>
        <description>Cancel Order Line</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel Line</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var ordLineStatus = &apos;{! OrderLine__c.Status__c}&apos;;
var ordLineId = &apos;{! OrderLine__c.Id}&apos;;
var proceed_with_cancel = false;
var userId = &apos;{!User.Name}&apos;;
var projectManager = &apos;{!OrderLine__c.Assigned_Project_Manger__c}&apos;;


if (userId != projectManager ) {
alert(&apos;You are not priveledged to cancel Order Line. Only Assigned Project Manager can trigger cancellation. USERID: &apos;+userId+&apos; PM: &apos;+projectManager+&apos;.&apos;);
} else {

if (ordLineStatus == &quot;Submitted&quot; || ordLineStatus == &quot;New&quot; || ordLineStatus == &quot;Configured&quot;) {
proceed_with_cancel = true;
} else {
alert(&apos;Order Line in status \&apos;&apos;+ordLineStatus+&apos;\&apos; can not be cancelled.&apos;);
}

}




if (proceed_with_cancel == true) {
var response = confirm(&apos;Do you want to cancel Order Line?&apos;);
if (response) {

var result = sforce.apex.execute(&quot;IntegrationOrder&quot;,&quot;processOLCancel&quot;, {ordLineId:ordLineId, ordLineStatus:ordLineStatus });
if(result[0] == &apos;OK&apos;){
alert(result[1]);
location.reload(true);
} else if(result[0] == &apos;TRNSFR&apos;){
window.top.location.href = &quot;https://&quot; + window.top.location.host + result[1];
} else {
alert(result[1]);
}

}

}</url>
    </webLinks>
