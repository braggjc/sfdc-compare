        <fullName>Reporting_Line_Type__c</fullName>
        <description>Line Type for Reporting (mapping of Line Type to one of the values needed in the Report)</description>
        <externalId>false</externalId>
        <formula>IF( AND (ISPICKVAL(Line_Type__c, &quot;New service&quot;), NOT(ISPICKVAL(Status__c, &quot;Cancelled&quot;))), 
&quot;Gross sales&quot;, 
IF( AND (OR(ISPICKVAL(Line_Type__c, &quot;Upgrade&quot;),ISPICKVAL(Line_Type__c, &quot;Technical Change&quot;)), NOT(ISPICKVAL(Status__c, &quot;Cancelled&quot;))), 
&quot;Upgrades&quot;, 
IF( AND (ISPICKVAL(Line_Type__c, &quot;Price Change&quot;), NOT(ISPICKVAL(Status__c, &quot;Cancelled&quot;))), 
&quot;Price changes&quot;, 
IF( AND (ISPICKVAL(Line_Type__c, &quot;Downgrade&quot;), NOT(ISPICKVAL(Status__c, &quot;Cancelled&quot;))), 
&quot;Downgrades&quot;, 
IF( ISPICKVAL(Line_Type__c, &quot;Disconnection&quot;), 
IF ( ISPICKVAL(Status__c, &quot;Cancelled&quot;), &quot;Win back&quot;, &quot;Disconnections&quot;), 
IF ( ISPICKVAL(Status__c, &quot;Cancelled&quot;), &quot;Cancellations&quot;, &quot;Rest&quot;) 
) 
) 
) 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Line Type for Reporting</inlineHelpText>
        <label>Reporting Line Type</label>
