        <fullName>CDD_Change_Blockade</fullName>
        <active>true</active>
        <description>https://helpdeskce.in.gtsce.com/browse/CECOLPL-46</description>
        <errorConditionFormula>IF(AND(NOT(ISBLANK(PRIORVALUE(CommittedDeliveryDate__c))), PRIORVALUE(CommittedDeliveryDate__c) != CommittedDeliveryDate__c ),true, false)</errorConditionFormula>
        <errorMessage>Commited Delivery Date can not be modified.</errorMessage>
