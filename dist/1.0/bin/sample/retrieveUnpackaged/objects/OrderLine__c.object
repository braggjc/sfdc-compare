    <validationRules>
        <fullName>Modification_of_Costs_after_Submit</fullName>
        <active>true</active>
        <description>Modification of Costs after Submit is not allowed</description>
        <errorConditionFormula>AND(OR( ISCHANGED(Valid_To__c), ISCHANGED(Opex_Monthly__c), ISCHANGED(Opex_One_off__c), ISCHANGED(Capex_One_off__c), ISCHANGED(Capex_Reuseable__c)), NOT(ISPICKVAL(OrderLookup__r.Status__c, &apos;New&apos;)) , NOT(ISPICKVAL(OrderLookup__r.Status__c, &apos;Budgetary Quotation&apos;)), NOT(ISPICKVAL(OrderLookup__r.Status__c, &apos;Firm Quotation&apos;)), NOT(ISPICKVAL(OrderLookup__r.Status__c, &apos;In Progress&apos;)), NOT(ISPICKVAL(OrderLookup__r.Status__c, &apos;On Hold&apos;)), NOT( $Profile.Name = &apos;System Administrator&apos;))</errorConditionFormula>
        <errorMessage>Modification of Proposal Costs section after Submit is not allowed</errorMessage>
