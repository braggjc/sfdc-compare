    <webLinks>
        <fullName>Configure_Selected_Line</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Configure Selected Line</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 

var records = {!GETRECORDIDS( $ObjectType.OrderLine__c )};
if (records.length != 1) alert(&apos;Please select exactly one Line to configure.&apos;);
else {
  var result = sforce.connection.query(&quot;Select Id, Status__c, CProduct__c, CProduct__r.Synchronized__c, CProduct__r.GTS_Location__c, Service_Bundle__c, OrderLookup__r.GTS_Location__c, OrderLookup__r.OrderType__c, Line_Type__c, Minimum_Service_Period__c, CurrencyIsoCode From OrderLine__c Where Id = &apos;&quot; + records [0] + &quot;&apos;&quot;);

  var orderlines = result.getArray(&apos;records&apos;);
  var url_arr = window.location.href.split(&quot;/&quot;);
  var otype = orderlines[0].OrderLookup__r.OrderType__c;
  var prodAv = orderlines[0].OrderLookup__r.GTS_Location__c;
  var LineType = orderlines[0].Line_Type__c;
  var lstatus = orderlines[0].Status__c;
  var msp = orderlines[0].Minimum_Service_Period__c;

  if(orderlines[0].CProduct__r.Synchronized__c == &apos;false&apos; &amp;&amp; (prodAv==&apos;GTS Slovakia&apos; || prodAv==&apos;GTS Hungary&apos; )){
    alert(&apos;Definition of the product is desynchronized with Billing. It has to be synchronized first&apos;);
  }
  else {
    if((lstatus != &quot;New&quot;) &amp;&amp; (lstatus != &quot;Configured&quot;))
      alert(&apos;You can only configure order lines that are &quot;New&quot; or &quot;Configured&quot;&apos;);
    else{
      if(LineType == &quot;Release&quot; || LineType == &quot;Suspend&quot; || LineType == &quot;Disconnection&quot;){
        alert(&apos;Order for Disconection / Release / Suspend can not be modified by Product Configurator&apos;);
      } else if(msp==&apos;&apos; &amp;&amp; prodAv==&apos;GTS Poland&apos;){
        alert(&apos;Minimum Service Period is not set fot the selected Order Line!&apos;);
      } else if(LineType == &quot;Upgrade&quot; || LineType == &quot;Downgrade&quot; || LineType == &quot;Price Change&quot; || LineType == &quot;Technical Change&quot; || LineType == &quot;Data Update&quot;){



		var valRes = &apos;true&apos;;
		if(LineType == &quot;Data Update&quot;){
			valRes = sforce.apex.execute(&quot;ModificationOrderMethods&quot;,&quot;checkIfRoleIsAllowed&quot;, {actionType:&quot;Data Update&quot;, action:&quot;Update&quot;});
		}
		if(valRes == &apos;false&apos;){
			alert(&apos;Your role is not allowed to upate Data Update Order Line.&apos;);
		}else{
			var res = &apos;NONE&apos;;
			do{
			   res = sforce.apex.execute(&quot;ModificationOrderMethods&quot;,&quot;prepareOrderLinesObjectsDivided&quot;, {ordLines:orderlines});

			   if(res == &apos;INTEGRATION_ERROR&apos;){
					break;
			   }
			} while (res != &apos;ALL&apos;);
			if(res == &apos;INTEGRATION_ERROR&apos;){
				alert(&apos;Unexpected error during connection with CSR database. Please try again later. \nIf problem persist please contact System Administrator.&apos;);
			}else{
				window.location.href = &quot;/apex/OrderAtrributesModification?olid=&quot; + records[0]; 
			}
		}
      } else if( LineType == &apos;Service Migration&apos;){
		sforce.apex.execute(&quot;ModificationOrderMethods&quot;,&quot;prepareOfferChangeOrderLine&quot;, {orderLineId:orderlines});
		window.location.href = &quot;/apex/OrderAtrributesModification?olid=&quot; + records[0]; 
	  }else if(LineType == &quot;New Service&quot; || LineType == &quot;Migration Installation&quot; || LineType == &quot;Cession&quot;){
        window.location.href = &quot;/apex/OrderAtrributesModification?olid=&quot; + records[0]; 
      }
    }
  }
}</url>
