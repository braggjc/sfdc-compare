    <validationRules>
        <fullName>BankAccountNameWithBANumber</fullName>
        <active>true</active>
        <description>Check, if Bank name has been selected then the Bank account number also has to be filled in</description>
        <errorConditionFormula>OR (AND(NOT (ISBLANK(Bank_Account_Number__c)), ISBLANK(BankName__c)),AND ((ISBLANK(Bank_Account_Number__c)), NOT (ISBLANK(BankName__c))))</errorConditionFormula>
        <errorDisplayField>BankName__c</errorDisplayField>
        <errorMessage>If you select a Bank name you have to enter Bank account number. If you enter Bank account number you also have to choose bank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckBankAccountForPaymentMethod</fullName>
        <active>true</active>
        <description>If as a Payement method Direct debit has been selected then Bank name, Bank account number and limit can not be empty</description>
        <errorConditionFormula>AND(PaymentMethod__r.Name = &apos;Direct debit&apos; ,OR( ISNULL(BankName__c) , ISNULL(Bank_Account_Number__c),  ISNULL( Limit__c )))</errorConditionFormula>
        <errorDisplayField>PaymentMethod__c</errorDisplayField>
        <errorMessage>Bank name, Bank account number and Limit are required if Direct debit is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckBankAccountForPaymentMethodBA</fullName>
        <active>true</active>
        <description>If as a Payment method Direct debit has been selected then Bank name, Bank account number and limit can not be empty</description>
        <errorConditionFormula>AND(PaymentMethod__r.Name = &apos;Direct Debit&apos; ,OR( ISBLANK(BankName__c) , ISBLANK(Bank_Account_Number__c), ISBLANK( Limit__c )))</errorConditionFormula>
        <errorDisplayField>PaymentMethod__c</errorDisplayField>
        <errorMessage>If as a Payement method Direct debit has been selected then Bank name, Bank account number and limit can not be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Address_Complete</fullName>
        <active>true</active>
        <description>If Complete Address is marked it checks whether 1st Line and/or 2nd Line is filled in.</description>
        <errorConditionFormula>IF( !ISPICKVAL( Account__r.GTS_Location_del__c, &apos;GTS Hungary&apos;)&amp;&amp;flgComplete__c = true &amp;&amp; IF(Line_1__c = &apos;&apos; &amp;&amp; Line_2__c = &apos;&apos;, true, false), true, false)</errorConditionFormula>
        <errorMessage>You have to fill either 1st Line or 2nd Line when Complete Address is checked!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Debtor_Group_90_validation</fullName>
        <active>true</active>
        <description>Only Billing Manager SK is available to set DG90</description>
        <errorConditionFormula>AND( 
ISCHANGED(Debtor_Group__c), 
ISPICKVAL( Debtor_Group__c, &apos;90&apos;) , $UserRole.Name !=&apos;Billing Manager SK&apos;)</errorConditionFormula>
        <errorDisplayField>Debtor_Group__c</errorDisplayField>
        <errorMessage>Only Billing Manager SK role is able to set Debtor Group 90</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_Address_required_for_Delivery_Type</fullName>
        <active>true</active>
        <description>Email address must be given if Invoice Delivery EMAIL selected</description>
        <errorConditionFormula>AND(OR( InvoiceDelivery__r.Value__c  = &apos;EMAIL&apos;, InvoiceDelivery__r.Value__c  = &apos;BOTH&apos;), ISBLANK( Invoice_Email__c ))</errorConditionFormula>
        <errorDisplayField>Invoice_Email__c</errorDisplayField>
        <errorMessage>Please, provide email address for invoice delivery</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HU_BlockBillingSystemChange</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Account__r.GTS_Location_del__c, &apos;GTS Hungary&apos;), NOT(ISNEW()), ISCHANGED( Billed_From__c ))</errorConditionFormula>
        <errorMessage>You are not allowed to changed assigned Billing System, after BA creation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HU_BlockCurrencyChange</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Account__r.GTS_Location_del__c, &apos;GTS Hungary&apos;), NOT(ISNEW()), ISCHANGED( CurrencyIsoCode))</errorConditionFormula>
        <errorMessage>You are not allowed to modify Billing Account Currency after its creation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HU_Invoice_delivery_check</fullName>
        <active>true</active>
        <description>checks the accepted method of invoice delivery for HU</description>
        <errorConditionFormula>AND(ISPICKVAL( Account__r.GTS_Location_del__c, &apos;GTS Hungary&apos;),InvoiceDelivery__r.Name &lt;&gt;&apos;Printed Invoice&apos;)</errorConditionFormula>
        <errorDisplayField>InvoiceDelivery__c</errorDisplayField>
        <errorMessage>The selected Invoice delivery method is not supported in HU. Please correct. the supported value is: Printed Invoice.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HU_invoice_language_check</fullName>
        <active>true</active>
        <description>Invoice language validation for Hungary</description>
        <errorConditionFormula>IF(
ISPICKVAL( Account__r.GTS_Location_del__c, &apos;GTS Hungary&apos;) &amp;&amp;
if(
OR((InvoiceLanguage__r.Name =&apos;English&apos;),(InvoiceLanguage__r.Name =&apos;Slovak&apos;)) ,true,false),true,false)</errorConditionFormula>
        <errorDisplayField>InvoiceLanguage__c</errorDisplayField>
        <errorMessage>The accepted invoice language for Hungary is: Hungarian. Please correct</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HU_payment_method_check</fullName>
        <active>true</active>
        <description>Checks the allowed payment method for Hungary</description>
        <errorConditionFormula>IF(
ISPICKVAL( Account__r.GTS_Location_del__c, &apos;GTS Hungary&apos;) &amp;&amp;
if(
OR( PaymentMethod__r.Name=&apos;Payment Order&apos;) ,true,false),true,false)</errorConditionFormula>
        <errorDisplayField>PaymentMethod__c</errorDisplayField>
        <errorMessage>The selected Payment Method is not handled in Hungary. Please correct.
Possible options are: Postal Payment Check, Bank Transfer or Direct Debit</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Modification_of_Active_Account_forbidden</fullName>
        <active>true</active>
        <description>It is not allowed to modify some fields above Pending status</description>
        <errorConditionFormula>AND ( OR (TEXT ( PRIORVALUE( CurrencyIsoCode ) ) != TEXT (CurrencyIsoCode), PRIORVALUE(  Price_List__c  ) != Price_List__c ), NOT (ISPICKVAL( Status__c , &apos;Pending&apos;) ) )</errorConditionFormula>
        <errorMessage>Fields Currency and Price list can be modified only on Pending status.</errorMessage>
    </validationRules>
    <validationRules>
