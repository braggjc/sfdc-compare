records = result.getArray(&quot;records&quot;);

var a = &apos;false&apos;;

var resultString = &apos;&apos;

 

for(var i = 0; i &lt; records.length; i ++){

        resultString = sforce.apex.execute(&quot;Bundle_Synchronization_SK&quot;,&quot;Synchronize_Bundle&quot;, {bundleId: records[i].Id, test: a});

