    <validationRules>
        <fullName>Column_Number</fullName>
        <active>true</active>
        <description>Checks if entered Column Number is not lower than 1</description>
        <errorConditionFormula>ColumnNo__c &lt; 1</errorConditionFormula>
        <errorDisplayField>ColumnNo__c</errorDisplayField>
        <errorMessage>Column Number must be higher than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Related_Field_Required</fullName>
        <active>true</active>
        <description>Requires value in Related Field if Related Object is not empty</description>
        <errorConditionFormula>AND(ISNULL(RelatedField__c),  NOT(ISNULL(RelatedObject__c)))</errorConditionFormula>
        <errorDisplayField>RelatedField__c</errorDisplayField>
        <errorMessage>Related Field is Required if Related Object was entered</errorMessage>
    </validationRules>
