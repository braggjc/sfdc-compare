    <deploymentStatus>Deployed</deploymentStatus>
    <description>Holds dependency definition between Provisioning Plan Task and Attribute that is required to close such Task</description>
    <enableActivities>false</enableActivities>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableReports>false</enableReports>
    <fields>
        <fullName>Attribute_Link__c</fullName>
        <externalId>false</externalId>
        <formula>HYPERLINK( &apos;/&apos;+ MandatoryAttribute__c , MandatoryAttribute__r.Name+IF(ISBLANK( MandatoryAttribute__r.Label_Eng__c ),&apos;&apos;,&apos; - &apos;+ MandatoryAttribute__r.Label_Eng__c ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Attribute Link</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Attribute_Name__c</fullName>
        <externalId>false</externalId>
        <formula>MandatoryAttribute__r.Name</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Attribute Name</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MandatoryAttribute__c</fullName>
        <externalId>false</externalId>
        <label>Mandatory Attribute</label>
        <referenceTo>Attribute__c</referenceTo>
        <relationshipLabel>Provisioning Plan Task Attributes</relationshipLabel>
        <relationshipName>Provisioning_Plan_Task_Attributes</relationshipName>
        <required>false</required>
