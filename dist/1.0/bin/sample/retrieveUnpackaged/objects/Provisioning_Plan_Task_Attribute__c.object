    <label>Provisioning Plan Task Attribute</label>
    <nameField>
        <displayFormat>PPTA{000000000}</displayFormat>
        <label>Provisioning Plan Task Attribute Name</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Provisioning Plan Task Attributes</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Mandatory_Attributes_For_Technical_Tasks</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c ,&apos;Update of Billing Products&apos;),ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c ,&apos;Billing Account Creation&apos;),ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c ,&apos;IP Unregistration&apos;),ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c ,&apos;IP Activation&apos;),ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c ,&apos;Phone Number Release&apos;),ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c ,&apos;Phone Number Activation&apos;),ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c ,&apos;Automatic Provisioning (ST)&apos;),ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c ,&apos;Automatic Provisioning&apos;),ISPICKVAL(Provisioning_Plan_Task__r.Task_Type__c ,&apos;Update of CSR Database&apos;))</errorConditionFormula>
        <errorMessage>Mandatory Attributes can be only required for manual Provisioning Plan Tasks.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Technical_and_Active_attributes_only</fullName>
        <active>true</active>
        <errorConditionFormula>OR(MandatoryAttribute__r.Active__c != True, MandatoryAttribute__r.Technical__c != True)</errorConditionFormula>
        <errorMessage>Only &apos;Active&apos; and &apos;Technical&apos; Attributes can be required for manual Provisioning Plan Tasks.</errorMessage>
    </validationRules>
</CustomObject>
