        <fullName>Version_validation</fullName>
        <active>true</active>
        <description>Check if version is numeric value (required because we must use . as decimal separator)</description>
        <errorConditionFormula>!REGEX(Version__c  , &quot;[0-9]*\\.?[0-9]+&quot;) || ( BEGINS(Version__c, &apos;0&apos;) &amp;&amp; !BEGINS(Version__c, &apos;0.&apos;) )</errorConditionFormula>
        <errorDisplayField>Version__c</errorDisplayField>
        <errorMessage>Version field must be a numeric value with . as decimal separator. (Example: 1.1)</errorMessage>
    </validationRules>
