        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var records = {!GETRECORDIDS($ObjectType.Project_cost__c)};
var OpportunityId = &quot;{!Opportunity.Id}&quot;;
var verified = &quot;{!Opportunity.Sales_Type_Verified__c}&quot;;
var ProjectId = &quot;&quot;;
var result;

if(verified == &quot;1&quot;){
   if (records.length != 1) {
      alert(&quot;Please select exactly one Project Cost to create Calculation from it&quot;);
   } else {
      ProjectId = records[0];
      window.location = window.location.protocol + &quot;//&quot; + window.location.host + &quot;/apex/CreateNewCalculation?opp_id=&quot; + OpportunityId + &quot;&amp;proj_id=&quot; + ProjectId;
   }
} else {
   alert(&quot;Sales Type must be verified before sending calculation to BCT&quot;);
}</url>
