        <fullName>Stage_Age__c</fullName>
        <description>Age in days of current Stage on Opportunity</description>
        <externalId>false</externalId>
        <formula>IF(IsClosed=false,TODAY() - Stage_Start_Date__c,null)</formula>
        <inlineHelpText>Age in days of current Stage on Opportunity</inlineHelpText>
        <label>Stage Age</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Stage_End_Date__c</fullName>
        <description>Last possible date to meet duration limit for stage</description>
        <externalId>false</externalId>
        <formula>If ( TEXT(GTS_Location__c) =&quot;GTS Poland&quot; &amp;&amp; $RecordType.Name =&quot;Domestic&quot;&amp;&amp;ISBLANK(Stage_Restart_Date__c) , 

CASE( TEXT(StageName), 
&quot;Suspect&quot;, Stage_Start_Date__c+21, 
&quot;Prospect&quot;, Stage_Start_Date__c+21, 
&quot;Proposal&quot;, Stage_Start_Date__c+42, 
&quot;Negotiation&quot;, Stage_Start_Date__c+42, 
&quot;Contract Signature&quot;,Stage_Start_Date__c+14,null), 

If ( TEXT(GTS_Location__c) =&quot;GTS Poland&quot; &amp;&amp; $RecordType.Name =&quot;Domestic&quot;&amp;&amp;NOT(ISBLANK(Stage_Restart_Date__c)), 

CASE( TEXT(StageName), 
&quot;Suspect&quot;, Stage_Restart_Date__c+21, 
&quot;Prospect&quot;, Stage_Restart_Date__c+21, 
&quot;Proposal&quot;, Stage_Restart_Date__c+42, 
&quot;Negotiation&quot;,Stage_Restart_Date__c+42, 
&quot;Contract Signature&quot;,Stage_Restart_Date__c+14,null), 
null))</formula>
        <inlineHelpText>Last possible date to meet duration limit for stage</inlineHelpText>
        <label>Stage End Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Date</type>
    </fields>
