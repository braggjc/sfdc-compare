    <fields>
        <fullName>Stage_Time_Remaining__c</fullName>
        <description>Number of Days after which the limit will be reached for current Stage. can be different per GTS Location</description>
        <externalId>false</externalId>
        <formula>If ( TEXT(GTS_Location__c) =&quot;GTS Poland&quot; &amp;&amp; $RecordType.Name =&quot;Domestic&quot;&amp;&amp;ISBLANK(Stage_Restart_Date__c) , 

CASE( TEXT(StageName), 
&quot;Suspect&quot;, 21 - Stage_Age__c , 
&quot;Prospect&quot;, 21 - Stage_Age__c, 
&quot;Proposal&quot;, 42 - Stage_Age__c, 
&quot;Negotiation&quot;, 42 - Stage_Age__c, 
&quot;Contract Signature&quot;,14 - Stage_Age__c, 0), 

If ( TEXT(GTS_Location__c) =&quot;GTS Poland&quot; &amp;&amp; $RecordType.Name =&quot;Domestic&quot;&amp;&amp;NOT(ISBLANK(Stage_Restart_Date__c)), 

CASE( TEXT(StageName), 
&quot;Suspect&quot;, 21 - (Today() -Stage_Restart_Date__c) , 
&quot;Prospect&quot;, 21 - (Today()- Stage_Restart_Date__c), 
&quot;Proposal&quot;, 42 - (Today()- Stage_Restart_Date__c), 
&quot;Negotiation&quot;, 42 - (Today()-Stage_Restart_Date__c), 
&quot;Contract Signature&quot;,14 - (Today()-Stage_Restart_Date__c), 0),null))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Number of Days after which the limit will be reached for current Stage</inlineHelpText>
        <label>Stage Time Remaining</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
