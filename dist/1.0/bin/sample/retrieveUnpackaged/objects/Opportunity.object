    <validationRules>
        <fullName>Active_Contact_is_required_for_Prospect</fullName>
        <active>true</active>
        <description>To change Stage from Suspect to Prospect Active Contact is required on Account
(Added temporray condition for GTS Location  GTS Slovakia becaus of PL migration)</description>
        <errorConditionFormula>IF ( TEXT(GTS_Location__c) = &quot;GTS Slovakia&quot;, 
((ISNEW() &amp;&amp;Account.Has_Contact__c =false) ||(Account.Has_Contact__c =false &amp;&amp;  ISCHANGED(StageName)) )&amp;&amp; TEXT(StageName)&lt;&gt;&quot;Suspect&quot; &amp;&amp; $RecordType.Name =&quot;Domestic&quot;&amp;&amp;  $User.ProfileId &lt;&gt;&quot;00e20000000qH2C&quot; &amp;&amp;  NOT(Simplified_Order__c) &amp;&amp;  NOT(CONTAINS(Name,&apos;Case Termination of&apos;))
, false)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>You are not allowed to change Stage from Suspect To Prospect if there is no Active contact on Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Block_creation_for_invalid_Accounts</fullName>
        <active>true</active>
        <description>when Domestic Opportunity is saved - rule verifies &quot;Account Validation&quot; field for related Account and if the value is &quot;Invalid&quot; or &quot;Unverified&quot; - blocks saving of Opportunity
(Added temporary condition GTS Location GTS Slovakia because of PL migration)</description>
        <errorConditionFormula>IF ( OR(TEXT(GTS_Location__c) = &quot;GTS Slovakia&quot;,TEXT(GTS_Location__c) = &quot;GTS Hungary&quot;), 

ISNEW() &amp;&amp; ( ISPICKVAL( Account.Account_Validation__c , &quot;&quot;)|| ISPICKVAL( Account.Account_Validation__c , &quot;Invalid&quot;)|| ISPICKVAL(Account.Account_Validation__c, &quot;Unverified&quot;)) &amp;&amp;TEXT(LeadSource)=&quot;&quot;&amp;&amp; $Profile.Id &lt;&gt;&quot;00e20000000qH2C&quot;

, false)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>&quot;Account Validation&quot; field value is: &quot;Invalid&quot; or &quot;Unverified&quot; at the related Account. 
Creation of Opportunity for such an Account is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CloseDate_vs_Status</fullName>
        <active>true</active>
        <description>Rule preventing Close Date change after Opportunity closed</description>
        <errorConditionFormula>AND
(ISCHANGED (CloseDate),
OR(ISPICKVAL (PRIORVALUE (StageName), &quot;Closed Won&quot;),
 ISPICKVAL (PRIORVALUE (StageName), &quot;Closed Lost&quot;),
ISPICKVAL (PRIORVALUE (StageName), &quot;Cancellation&quot;)),or ($Profile.Name &lt;&gt; &quot;System Administrator&quot;),($Profile.Name &lt;&gt; &quot;GTS CE CEO&quot;))
&amp;&amp;$RecordType.Name&lt;&gt;&quot;Domestic&quot;&amp;&amp;$RecordType.Name&lt;&gt;&quot;Wholesale Voice&quot;</errorConditionFormula>
        <errorMessage>You can&apos;t change Close Date field when Stage field is Closed Won or Closed Lost or Cancelation. If the opportunity close date needs to be changed, please contact Artur Ostrowski.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Won_rule_for_RO</fullName>
        <active>true</active>
        <description>Based on Requirement from Laura Constantinescu.</description>
        <errorConditionFormula>AND(
ISPICKVAL(GTS_Location__c,&apos;GTS Romania&apos;),
ISCHANGED(StageName),
ISPICKVAL(Sales_Type__c,&apos;Retail&apos;),
ISPICKVAL(StageName,&apos;Closed Won&apos;), 
$UserRole.Name&lt;&gt;&apos;Domestic Sales Director RO&apos;
)</errorConditionFormula>
        <errorMessage>Only RO Domestic Sales managers are able to set the opportunity stage to Closed Won!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Decreasing_Probability_not_allowed</fullName>
        <active>false</active>
        <description>Prevents from decreasing the Probablility</description>
        <errorConditionFormula>AND( Probability &lt; PRIORVALUE(Probability) , OR($RecordType.Name = &apos;Domestic&apos;, $RecordType.Name = &apos;Domestic Service Order&apos;))</errorConditionFormula>
        <errorDisplayField>Probability</errorDisplayField>
        <errorMessage>You are not allowed to decrease the Probability</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Domestic_Opportunities_GTS_Location_chec</fullName>
        <active>true</active>
        <description>Only the same GTS Location can be chosen as on Account for Domestic Opportunities</description>
        <errorConditionFormula>$RecordType.Name = &apos;Domestic&apos; &amp;&amp; ( Text(Account.GTS_Location_del__c) != Text(GTS_Location__c) ) &amp;&amp; Text(GTS_Location__c) !=&apos;&apos;</errorConditionFormula>
        <errorDisplayField>GTS_Location__c</errorDisplayField>
        <errorMessage>GTS Location on Opportunity should be the same as on Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Close_Date_changed_opp</fullName>
        <active>true</active>
        <description>Domestic Opportunities rule. Attempt to Close Opportunity with date from last week or next week will be blocked unless Close Date Override checkbox is marked</description>
        <errorConditionFormula>IsChanged (IsClosed) &amp;&amp; 
IsClosed = true &amp;&amp; 
Close_This_Week__c = &quot;false&quot; &amp;&amp; 
!CONTAINS($UserRole.Name,&quot;Sales Support&quot;) &amp;&amp; 
!CONTAINS($Profile.Name,&quot;System Administrator&quot;) &amp;&amp;
Simplified_Order__c = false &amp;&amp;
$RecordType.Name =&quot;Domestic&quot; &amp;&amp; 
!CONTAINS(Name, &quot;Auto generated from Case&quot;) &amp;&amp; 
!ISPICKVAL( GTS_Location__c, &quot;GTS Czech Rep&quot;)</errorConditionFormula>
        <errorMessage>You are not allowed to close Opportunity with Close Date from previous weeks or upcoming weeks.
Mark Close Date Override if entered Close Date is correct and you would like Opportunity to be closed by Sales Support.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Close_Date_new_opp</fullName>
        <active>true</active>
        <description>Domestic Opportunities rule. Attempt to Close Opportunity with date from last week or next week will be blocked unless Close Date Override checkbox is marked</description>
        <errorConditionFormula>ISNEW() &amp;&amp; IsClosed = true &amp;&amp;
Close_This_Week__c = &quot;false&quot; &amp;&amp;
!CONTAINS($UserRole.Name,&quot;Sales Support&quot;) &amp;&amp;
!CONTAINS($Profile.Name,&quot;System Administrator&quot;) &amp;&amp;
Simplified_Order__c = false &amp;&amp;
$RecordType.Name =&quot;Domestic&quot; &amp;&amp;
!CONTAINS(Name, &quot;Auto generated from Case&quot;) &amp;&amp;
!ISPICKVAL( GTS_Location__c, &quot;GTS Czech Rep&quot;)</errorConditionFormula>
        <errorMessage>You are not allowed to close Opportunity with Close Date from previous weeks or upcoming weeks.
Mark Close Date Override if entered Close Date is correct and you would like Opportunity to be closed by Sales Support.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PL_CreditCheck_ClosedWon</fullName>
        <active>false</active>
        <description>Closed Won Stage can be set only when verification completed</description>
        <errorConditionFormula>AND( ISCHANGED( StageName ) , ISPICKVAL(StageName , &apos;Closed Won&apos;) , CC_Credit_Verified__c = true, NOT(ISPICKVAL(Opportunity_Source__c,&apos;Extranet Weborder&apos;)), ISPICKVAL(CC_Credit_Decision__c, &apos;Rejected&apos;))</errorConditionFormula>
        <errorMessage>You can not close Opportunity as won if Credit Verification was Rejected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PL_CreditCheck_Completion</fullName>
        <active>true</active>
        <description>Only Collection User can complete Verification with Red or Black Flag</description>
        <errorConditionFormula>AND( OR( ISPICKVAL( CC_Credit_Flag__c, &apos;Red&apos; ) , ISPICKVAL( CC_Credit_Flag__c, &apos;Black&apos;)) , $Profile.Name != &apos;Collection&apos;, $Profile.Name != &apos;System Administrator&apos;, ISCHANGED( CC_Credit_Verified__c ), CC_Credit_Verified__c = true)</errorConditionFormula>
        <errorMessage>You are not allowed to Complete verification when flag is Red or Black</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PL_CreditCheck_Guarantee_Type_and_Amoun</fullName>
        <active>true</active>
        <description>If one of those fields is filled, second is also required</description>
        <errorConditionFormula>OR( 
AND( ISBLANK( CC_Deposit_Amount__c ), NOT(ISPICKVAL( CC_Guarantee_Type__c , &quot;&quot;))), 
AND( NOT(ISBLANK( CC_Deposit_Amount__c )), ISPICKVAL( CC_Guarantee_Type__c , &quot;&quot;) ) 
)</errorConditionFormula>
        <errorDisplayField>CC_Deposit_Amount__c</errorDisplayField>
        <errorMessage>If one of the fields Guarantee Type, Guarantee Amount is filled, second one is also required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PL_CreditCheck_Restriction</fullName>
        <active>true</active>
        <description>Sales is not allowed to modify fields when Red or Black flag was set</description>
        <errorConditionFormula>AND( OR( ISPICKVAL( PRIORVALUE(CC_Credit_Flag__c), &apos;Red&apos; ) , ISPICKVAL( PRIORVALUE(CC_Credit_Flag__c), &apos;Black&apos;)) , $Profile.Name != &apos;Collection&apos;, $Profile.Name != &apos;System Administrator&apos;, OR(ISCHANGED( CC_Verification_Comment__c ), ISCHANGED( CC_Deposit_Amount__c ), ISCHANGED( CC_Credit_Decision__c ), ISCHANGED( CC_Guarantee_Type__c ), ISCHANGED(CC_Credit_Flag__c)))</errorConditionFormula>
        <errorMessage>You are not allowed to make modifications when flag is set to Red or Black</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PL_CreditCheck_Restriction2</fullName>
        <active>true</active>
        <description>Sales is not allowed to modify fields when Orange flag was set</description>
        <errorConditionFormula>AND( 
ISPICKVAL( PRIORVALUE(CC_Credit_Flag__c), &apos;Orange&apos; ), 
$Profile.Name != &apos;Collection&apos;, 
$Profile.Name != &apos;System Administrator&apos;, 
$User.Id != OwnerUser__r.ManagerId, 
OR( 
ISCHANGED( CC_Verification_Comment__c ), 
ISCHANGED( CC_Deposit_Amount__c ), 
ISCHANGED( CC_Guarantee_Type__c ), 
ISCHANGED( CC_Credit_Decision__c ), 
ISCHANGED( CC_Credit_Flag__c) 
) 
)</errorConditionFormula>
        <errorMessage>You are not allowed to make modifications when flag is set to Orange</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PL_SalesTypeVerification_Required</fullName>
        <active>true</active>
        <description>Sales Type verification required for Closed Won Stage</description>
        <errorConditionFormula>AND( 
Sales_Type_Verified__c = false, 
ISCHANGED(StageName), 
