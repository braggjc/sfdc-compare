    <webLinks>
        <fullName>Set_Primary</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Set Primary</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/apex.js&quot;)} 

var records = {!GETRECORDIDS( $ObjectType.Contact )};
if (records.length == 0) alert(&apos;Please select at least one contact.&apos;);
else if (records.length &gt; 1 ) {
    alert (&apos;Only one contact can be set to primary.&apos;);
}

else if (records.length == 1) {
    var result = sforce.connection.query(&quot;Select Id,  Primary_contact__c from Contact where Primary_contact__c = true&quot;);

    var contacts = result.getArray(&quot;records&quot;);

    if (contacts.length&gt;0){
        for (var i = 0; i &lt; contacts.length; i++){
            var oldPrimaryContact = contacts[i];
            oldPrimaryContact.Primary_contact__c = false;
            sforce.connection.update([oldPrimaryContact]);
        }
    }

    result = sforce.connection.query(&quot;Select Id,  Primary_contact__c from Contact where Id =&apos;&quot; + records[0] + &quot;&apos;&quot;);
    var cont = result.getArray(&quot;records&quot;)[0]; 

    cont.Primary_contact__c = true;
    sforce.connection.update([cont]);

    //reload the current page
    window.location.href = window.location.href;

}</url>
    </webLinks>
