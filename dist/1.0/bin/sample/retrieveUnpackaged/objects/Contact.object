    <validationRules>
        <fullName>Email_Phone_or_Mobile_required</fullName>
        <active>true</active>
        <description>for Contacts assigned to Company and Partner Accounts</description>
        <errorConditionFormula>IF ( 
	OR (
		CONTAINS( Account.RecordTypeId , &quot;01220000000LWct&quot; ), 
		CONTAINS( Account.RecordTypeId , &quot;01220000000LWcv&quot; ) 
	),

	AND (ISBLANK ( Email ), ISBLANK ( Phone ), ISBLANK ( MobilePhone ) ), 

False)</errorConditionFormula>
        <errorMessage>Either Phone or Mobile or Email field must be filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Extranet_Role_Vs_Account</fullName>
        <active>false</active>
        <description>Validates if  selected role corresponds to related Account</description>
        <errorConditionFormula>((TEXT(Extranet_Role__c) = &quot;Customer Admin User&quot; || TEXT(Extranet_Role__c) = &quot;Customer Regular User&quot;) &amp;&amp; NOT(CONTAINS( Account.RecordTypeId,  &quot;01220000000LWct&quot;)) &amp;&amp;  NOT(CONTAINS( Account.RecordTypeId,  &quot;01220000000LX4P&quot;)))||
((TEXT(Extranet_Role__c) = &quot;Dealer Admin User&quot; || TEXT(Extranet_Role__c) = &quot;Dealer  Regular User&quot;) &amp;&amp; (NOT(CONTAINS( Account.RecordTypeId,  &quot;01220000000LWcv&quot;)) ||   Account.Dealer__c =false))</errorConditionFormula>
        <errorDisplayField>Extranet_Role__c</errorDisplayField>
        <errorMessage>Extranet Role must correspond to Account:
- Customer User  only for Comapny or Residential
- Partner Dealer User only for Partner and chekbox Dealer set to true</errorMessage>
    </validationRules>
