    <validationRules>
        <fullName>Reason_of_Delay_flag_change</fullName>
        <active>false</active>
        <description>Reason of Delay flag can be changed on Provisioning Plan definition only (not instance).</description>
        <errorConditionFormula>AND(Instance__c = true, ISCHANGED( Reason_of_Delay__c ) )</errorConditionFormula>
        <errorDisplayField>Reason_of_Delay__c</errorDisplayField>
        <errorMessage>Reason of Delay flag can be changed only on Provisioning Plan definition (not instance).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Starting_Datetime_on_instance</fullName>
        <active>false</active>
        <description>Starting Datetime can be set on plan instance only</description>
        <errorConditionFormula>IF(AND(NOT(ISBLANK(Starting_Datetime__c)), Instance__c=false), true, false)</errorConditionFormula>
        <errorDisplayField>Starting_Datetime__c</errorDisplayField>
        <errorMessage>Starting Datetime can be set on Provisioning Plan instance only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Starting_Datetime_past</fullName>
        <active>false</active>
        <description>Starting Datetime can not be set in past.</description>
        <errorConditionFormula>Starting_Datetime__c &lt; NOW()</errorConditionFormula>
        <errorDisplayField>Starting_Datetime__c</errorDisplayField>
        <errorMessage>Starting Datetime can not be set in past.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Back_To_Assignment</fullName>
        <availability>online</availability>
        <description>Allows to go back directly to Provisioning Plan Assignment Page</description>
        <displayType>link</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Back To Assignment</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var bInstance = &apos;{!Provisioning_Plan__c.Instance__c}&apos;;
var sLineStatus = &apos;{!Provisioning_Plan__c.Order_Line_Status__c}&apos;;

if(bInstance == &apos;1&apos;){
  if(sLineStatus != &apos;Submitted&apos; &amp;&amp; sLineStatus != &apos;Completed&apos;){
    window.location.href = &apos;apex/AssignProvisioningPlans?ordId={!Provisioning_Plan__c.Order_Id__c}&apos;;
  } else {
    alert(&apos;You can go back to Assignment only for not Submitted Orders&apos;);
  }
} else {
  alert(&apos;You can go back to Assignment only from Instance and not Definition&apos;);
}</url>
    </webLinks>
    <webLinks>
        <fullName>Calculate_Due_Dates</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Calculate Due Dates</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var provPlanId = &apos;{!Provisioning_Plan__c.Id}&apos;;
var firstId = &apos;{!Provisioning_Plan__c.Id}&apos;;

var validation_result = sforce.apex.execute(&quot;ProvisioningPlanNew&quot;,&quot;validatePlanStructureBeforeCalculationWS&quot;, {provPlanId:provPlanId});

if(validation_result[0] == &apos;ERROR&apos;){
alert (validation_result[1]);
}else{
var result = sforce.apex.execute(&quot;ProvisioningPlanNew&quot;,&quot;calculateProvisioningPlanTaskDueDatesWS&quot;, {provPlanId:provPlanId});
if(result == &apos;0&apos;){
alert(&apos;Due Dates calculated correctly for this plan and other related child/parent plan(s).&apos;);
window.top.location.href = firstId;
} else if (result == &apos;1&apos;) {
alert(&apos;Error. You can calculate Due Dates for Plan instance only.&apos;);
} else if (result==&apos;2&apos;) {
alert(&apos;Error. There are no tasks!&apos;);
} else if (result==&apos;3&apos;) {
alert(&apos;Error. There are dependencies between tasks!!&apos;);
}else{
alert(&apos;Error while calculating Due Dates.&apos;);

}
}</url>
    </webLinks>
    <webLinks>
        <fullName>Clone</fullName>
        <availability>online</availability>
        <description>Clones Plan and related Tasks</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Clone</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 

var PlanID = &apos;{!Provisioning_Plan__c.Id}&apos;; 
var result = sforce.apex.execute(&quot;ProvisioningPlanCloning&quot;,&quot;clone&quot;, {sPlanID:PlanID}); 

if(result[0].length == 18){
window.top.location.href = &quot;https://&quot; + window.top.location.host + &quot;/&quot; + result; 
} else {
alert(result);
}</url>
    </webLinks>
    <webLinks>
        <fullName>Delete_Due_Dates</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Delete Due Dates</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var provPlanId = &apos;{!Provisioning_Plan__c.Id}&apos;;
if (confirm(&apos;Do you really want to delete all Due Dates from Provisioning Plan Tasks?&apos;)) {
var result = sforce.apex.execute(&quot;ProvisioningPlanNew&quot;,&quot;clearAllDueDatesWS&quot;, {provPlanId:provPlanId});
if(result[0] == &apos;OK&apos;){
window.location.reload()
} else {
alert(result[1]);
}
}</url>
    </webLinks>
    <webLinks>
        <fullName>Run_Validation</fullName>
        <availability>online</availability>
        <description>[PROV]_PL_064: PP verification</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Run Validation</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)} 

var ppID = &apos;{!Provisioning_Plan__c.Id}&apos;; 

var resultString = sforce.apex.execute(&quot;ProvisioningPlanMethods_Extension&quot;,&quot;validatePP&quot;, {ppId: ppID}); 
if(resultString[0]!=&apos;OK&apos;){ 
alert(resultString[1]); 
} else { 
alert(&apos;Provisioning Plan went through the validation succesfully. No problems has been found.&apos;); 
}</url>
    </webLinks>
