    <webLinks>
        <fullName>Configure</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Configure</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var morderstatus = &apos;{!Simplified_Order__c.Status__c}&apos;;
var morderid = &apos;{!Simplified_Order__c.Id}&apos;;

if(morderstatus == &quot;New&quot;) {
	alert(&apos;Initial target configuration is not ready yet. First submit the migration order.&apos;);
} else if (morderstatus == &quot;Submitted&quot;) {
	alert(&apos;The provisioning is already in progress and the configuration cannot be changed.&apos;);
} else if (morderstatus == &quot;Completed&quot;) {
	alert(&apos;This order has already been completed and configuration cannot be changed.&apos;);
} else if (morderstatus == &quot;Prepared&quot; || morderstatus == &quot;Configured&quot;) {
	var lineType = &apos;{!Simplified_Order__c.Line_Type__c}&apos;; 
	var result1 = sforce.connection.query(&quot;Select Installation_Order__r.Id From Simplified_Order__c Where Id = &apos;&quot; + morderid + &quot;&apos;&quot;);
	var morder = result1.getArray(&apos;records&apos;);
	var installationorderid = morder[0].Installation_Order__r.Id;
	var result = sforce.connection.query(&quot;Select Id, Status__c, CProduct__c, CProduct__r.Synchronized__c, CProduct__r.GTS_Location__c, Service_Bundle__c, OrderLookup__r.GTS_Location__c, OrderLookup__r.OrderType__c, Line_Type__c, Minimum_Service_Period__c, CurrencyIsoCode From OrderLine__c Where OrderLookup__c = &apos;&quot; + installationorderid + &quot;&apos;&quot;);
	var orderlines = result.getArray(&apos;records&apos;);
	if(lineType == &apos;New Service&apos;){
		window.location.href = &quot;/apex/OrderAtrributesModification?olid=&quot; + orderlines[0].Id;
	}else{
		var res = &apos;NONE&apos;;
		do{
		   res = sforce.apex.execute(&quot;ModificationOrderMethods&quot;,&quot;prepareOrderLinesObjectsDivided&quot;, {ordLines:orderlines});
		   if(res == &apos;INTEGRATION_ERROR&apos;){
				break;
			}
		} while (res != &apos;ALL&apos;);
		if(res == &apos;INTEGRATION_ERROR&apos;){
			alert(&apos;Unexpected error during connection with CSR database. Please try again later. \nIf problem persist please contact System Administrator.&apos;);
		}else{
			window.location.href = &quot;/apex/OrderAtrributesModification?olid=&quot; + orderlines[0].Id; 
		}
	}
}</url>
    </webLinks>
