    <validationRules>
        <fullName>Account_Owner_Modification</fullName>
        <active>true</active>
        <description>Modification of Account Owner allowed only for some Roles</description>
        <errorConditionFormula>AND( OwnerId != PRIORVALUE(OwnerId ) , ISPICKVAL( GTS_Location_del__c , &apos;GTS Slovakia&apos;) , $UserRole.Name != &apos;Domestic Sales Director SK&apos;, $UserRole.Name != &apos;Domestic Sales Support Director SK&apos;, $UserRole.Name != &apos;Domestic Sales Support SK&apos;,  $UserRole.Name != &apos;Front Office Manager SK&apos;,$UserRole.Name != &apos;Front Office Agent SK&apos; , $UserRole.Name != &apos;GTS CE IS VP&apos;,  $Profile.Name !=&apos;System Administrator&apos; )</errorConditionFormula>
        <errorDisplayField>OwnerId</errorDisplayField>
        <errorMessage>You are not allowed to change Account Owner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_Validation_security</fullName>
        <active>true</active>
        <description>Only selected users are allowed to change
(added temporary condition GTS Location GTS Slovakia because of PL migration)</description>
        <errorConditionFormula>( 
( 
($RecordType.Name = &quot;Company&quot; || 
$RecordType.Name = &quot;Partner&quot; || 
$RecordType.Name = &quot;Residential&quot; 
)&amp;&amp; 
( 
(ISNEW()&amp;&amp;TEXT(Account_Validation__c)&lt;&gt;&quot;Unverified&quot;&amp;&amp;$Profile.Id &lt;&gt;&quot;00e20000000qH2C&quot; &amp;&amp; Account_from_Lead__c = false&amp;&amp; TEXT( GTS_Location_del__c) &lt;&gt; &quot;GTS Slovakia&quot;)|| 
(ISNEW()&amp;&amp;TEXT(Account_Validation__c)&lt;&gt;&quot;Valid&quot;&amp;&amp;$Profile.Id &lt;&gt;&quot;00e20000000qH2C&quot; &amp;&amp; Account_from_Lead__c = true &amp;&amp; 
	((TEXT(GTS_Location_del__c) = &quot;GTS Poland&quot; &amp;&amp;$UserRole.Id &lt;&gt;&quot;00E200000012sdk&quot;&amp;&amp; $UserRole.Id &lt;&gt;&quot;00E20000000sPgg&quot;) ||
	(TEXT(GTS_Location_del__c) = &quot;GTS Czech Rep&quot; &amp;&amp; $UserRole.Id &lt;&gt;&quot;00E20000000sPga&quot; &amp;&amp; $Profile.Id &lt;&gt;&quot;00e20000000qH2C&quot;))
)
|| (ISCHANGED(Account_Validation__c) &amp;&amp; (TEXT(Account_Validation__c)= null || TEXT(Account_Validation__c)= &quot;&quot;)  &amp;&amp; TEXT(GTS_Location_del__c) = &quot;GTS Slovakia&quot; &amp;&amp; $Profile.Id &lt;&gt;&quot;00e20000000qH2C&quot;)
||
(ISCHANGED(Account_Validation__c) &amp;&amp; TEXT(GTS_Location_del__c) = &quot;GTS Poland&quot; &amp;&amp; $UserRole.Id &lt;&gt;&quot;00E20000000sPgg&quot;&amp;&amp;$UserRole.Id &lt;&gt;&quot;00E200000012sdk&quot; &amp;&amp; $Profile.Id &lt;&gt;&quot;00e20000000qH2C&quot;) || 
(ISCHANGED(Account_Validation__c) &amp;&amp; TEXT(GTS_Location_del__c) = &quot;GTS Czech Rep&quot; &amp;&amp; $UserRole.Id &lt;&gt;&quot;00E20000000sPga&quot; &amp;&amp; $Profile.Id &lt;&gt;&quot;00e20000000qH2C&quot;) 
) 
)|| 
( 
$RecordType.Name = &quot;International Wholesale&quot; &amp;&amp; ((ISCHANGED(Account_Validation__c) &amp;&amp; $UserRole.Id &lt;&gt;&quot;00E20000000sNDK&quot;&amp;&amp; $Profile.Id &lt;&gt;&quot;00e20000000qH2C&quot;) || 
(ISNEW()&amp;&amp;TEXT(Account_Validation__c)&lt;&gt;&quot;Unverified&quot;)) &amp;&amp;$Profile.Id &lt;&gt;&quot;00e20000000qH2C&quot; 
) 
)</errorConditionFormula>
        <errorDisplayField>Account_Validation__c</errorDisplayField>
        <errorMessage>1. To Create Account from Lead it has to be Valid - has to have all required data.
2. Account on Acccounts Tab can be created only with Unverified value.
3. Account Validation can be changed on Account only by Sales Support or Administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Address_is_required_for_Valid_Accounts</fullName>
        <active>true</active>
        <description>Change to Valid is not allowed if there is no Registration Address or it is incorrectly specified (i.e. missing in Account Contact section)

(addded temporary condition GTS Location GTS Slovakia because of PL migration )</description>
        <errorConditionFormula>IF ( TEXT( GTS_Location_del__c ) = &quot;GTS Slovakia&quot;, 
